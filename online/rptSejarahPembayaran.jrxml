     TBLPEMBAYARANONLINE."NO_RUJUKAN" AS TBLPEMBAYARANONLINE_NO_RUJUKAN
FROM
     "EKPTGDEV"."TBLRUJJENISBAYARAN" TBLRUJJENISBAYARAN INNER JOIN "EKPTGDEV"."TBLPEMBAYARANONLINE" TBLPEMBAYARANONLINE ON TBLRUJJENISBAYARAN."ID_JENISBAYARAN" = TBLPEMBAYARANONLINE."ID_JENISBAYARAN"
     INNER JOIN "EKPTGDEV"."TBLRUJSTATUS" TBLRUJSTATUS ON TBLPEMBAYARANONLINE."ID_STATUS" = TBLRUJSTATUS."ID_STATUS"

WHERE TBLPEMBAYARANONLINE."TARIKH_PEMBAYARAN" LIKE '%$P!{dariBulan}%' 
OR TBLPEMBAYARANONLINE."TARIKH_PEMBAYARAN" LIKE '%$P!{keBulan}%']]></queryString>

