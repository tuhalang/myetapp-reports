	<parameter name="ReportDir" class="java.lang.String"/>
	<parameter name="idfail" class="java.lang.String"/>
	<parameter name="namaPegawai" class="java.lang.String"/>
	<parameter name="jawatan" class="java.lang.String"/>
	<parameter name="namaPentadbir" class="java.lang.String"/>
	<parameter name="flagBorangF" class="java.lang.String"/>
	<parameter name="jumlahBayaran" class="java.lang.String" isForPrompting="false"/>
	<parameter name="statusFail" class="java.lang.String"/>
	<parameter name="flag_keb_kemaskini" class="java.lang.String"/>
	<parameter name="NILAI_BERSIH_1" class="java.lang.String"/>
	<parameter name="NILAI_BERSIH_2" class="java.lang.String"/>
	<parameter name="namaPentadbirBorangF" class="java.lang.String"/>
	<parameter name="noFailremoveslash" class="java.lang.String"/>
	<queryString>
		<![CDATA[-----/Borang E2 edit by wan on 25/10/2011 */------

SELECT DISTINCT
            a.ID_FAIL,
            Upper(REPLACE(A.NO_FAIL,'&','&#38;'))As No_Fail,

            B.Id_Permohonan,
            To_Char(B.Tarikh_Mohon,'yyyy') As Tahun,
            nmhn.NAMA_NEGERI AS NEGERI_mohon,
            REPLACE(dmhn.NAMA_DAERAH,'&','&#38;') AS DAERAH_mohon,
            Nvl(Replace(To_Char(B.Jumlah_Hta_Tarikhmati,'999,999,999.99'),' ', ''),' ')As Jumlah_Hta_Tarikhmati,

            c.ID_PERMOHONANSIMATI,

 ------ /* Maklumat Simati */ ------------
            d.ID_SIMATI,
            CASE
                WHEN d.NAMA_SIMATI IS NOT NULL AND d.NAMA_LAIN IS NULL THEN REPLACE(d.NAMA_SIMATI,'&','&#38;')
                WHEN d.NAMA_SIMATI IS NOT NULL AND d.NAMA_LAIN IS NOT NULL THEN REPLACE(d.NAMA_SIMATI,'&','&#38;') || ' @'|| REPLACE(d.NAMA_LAIN,'&','&#38;')
                WHEN d.NAMA_SIMATI IS NULL AND d.NAMA_LAIN IS NOT NULL THEN REPLACE(d.NAMA_LAIN,'&','&#38;')
            End As Nama_Simati,
            Case
 		WHEN (d.no_kp_baru IS NOT NULL OR d.no_kp_lama IS NOT NULL) THEN ', No. KP : '
                --WHEN d.jenis_kp in (0,5,6,7) THEN ', No. KP : '
                WHEN (d.no_kp_baru IS NULL and d.no_kp_lama is NULL and d.no_kp_lain IS NOT NULL and d.jenis_kp <> 4) THEN ', No. KP : '
                WHEN d.jenis_kp=4 and (d.no_kp_baru IS NULL OR d.no_kp_lama IS NULL) THEN ', No. PASPORT : '
            -- null,6,0,5,7,4,
                Else ''
            END AS Jenis_kp_simati,
            CASE
                WHEN d.no_kp_baru IS NOT NULL AND d.no_kp_lama IS NULL and d.no_kp_lain IS NULL THEN SUBSTR(d.no_kp_baru,1,6) || '-' || SUBSTR(d.no_kp_baru,7,2) || '-' || SUBSTR(d.no_kp_baru,9,4)
                WHEN d.no_kp_baru IS NOT NULL AND d.no_kp_lama IS NOT NULL AND d.no_kp_lain IS NULL THEN SUBSTR(d.no_kp_baru,1,6) || '-' || SUBSTR(d.no_kp_baru,7,2) || '-' || SUBSTR(d.no_kp_baru,9,4) || ' ('||d.no_kp_lama ||')'
                WHEN d.no_kp_baru IS NOT NULL AND d.no_kp_lama IS NOT NULL AND d.no_kp_lain IS NOT NULL AND d.jenis_kp <> 4 THEN SUBSTR(d.no_kp_baru,1,6) || '-' || SUBSTR(d.no_kp_baru,7,2) || '-' || SUBSTR(d.no_kp_baru,9,4) || ' ('||d.no_kp_lama ||' / '||d.no_kp_lain ||')'
                WHEN d.no_kp_baru IS NOT NULL AND d.no_kp_lama IS NOT NULL AND d.no_kp_lain IS NOT NULL AND d.jenis_kp = 4 THEN SUBSTR(d.no_kp_baru,1,6) || '-' || SUBSTR(d.no_kp_baru,7,2) || '-' || SUBSTR(d.no_kp_baru,9,4) || ' ('||d.no_kp_lama ||' ) No. PASPORT : '||d.no_kp_lain ||''
                WHEN d.no_kp_baru IS NOT NULL AND d.no_kp_lama IS NULL AND d.no_kp_lain IS NOT NULL AND d.jenis_kp = 4 THEN SUBSTR(d.no_kp_baru,1,6) || '-' || SUBSTR(d.no_kp_baru,7,2) || '-' || SUBSTR(d.no_kp_baru,9,4) || ' No. PASPORT : '||d.no_kp_lain ||''
                WHEN d.no_kp_baru IS NOT NULL AND d.no_kp_lama IS NULL and d.no_kp_lain IS NOT NULL AND d.jenis_kp <> 4 THEN SUBSTR(d.no_kp_baru,1,6) || '-' || SUBSTR(d.no_kp_baru,7,2) || '-' || SUBSTR(d.no_kp_baru,9,4)  || ' ('||d.no_kp_lain ||')'
                WHEN d.no_kp_baru IS NULL AND d.no_kp_lama IS NOT NULL and d.no_kp_lain IS NULL THEN REPLACE(d.no_kp_lama,'&','&#38;')
                WHEN d.no_kp_baru IS NULL AND d.no_kp_lama IS NOT NULL and d.no_kp_lain IS NOT NULL AND d.jenis_kp = 4 THEN REPLACE(d.no_kp_lama,'&','&#38;') || ' No. PASPORT : '||d.no_kp_lain ||''
                WHEN d.no_kp_baru IS NULL AND d.no_kp_lama IS NOT NULL and d.no_kp_lain IS NOT NULL AND d.jenis_kp <> 4 THEN REPLACE(d.no_kp_lama,'&','&#38;') || ' ('||d.no_kp_lain ||')'
                WHEN d.no_kp_baru IS NULL AND d.no_kp_lama IS NULL and d.no_kp_lain IS NOT NULL THEN REPLACE(d.no_kp_lain,'&','&#38;')
                ELSE ''
            END AS kp_simati,
            d.JENIS_WARGA AS WARGA_SIMATI,
            CASE
                WHEN d.ID_BUKTIMATI = 1 THEN ', No. Sijil Mati: '|| d.NO_SIJIL_MATI
                WHEN d.ID_BUKTIMATI = 2 THEN ', Surat Sumpah Kematian: '|| d.NO_SIJIL_MATI
                WHEN d.ID_BUKTIMATI = 3 THEN ', Anggapan Kematian dari Mahkamah Tinggi: '|| d.NO_SIJIL_MATI
                WHEN d.ID_BUKTIMATI = 4 THEN ', No. Permit Kubur: '|| d.NO_SIJIL_MATI
                ELSE ''
            END AS BUKTI_MATI,
            CASE
                WHEN TO_CHAR(d.TARIKH_MATI,'dd/mm/yyyy') < '01/11/1991' THEN '1'
                ELSE '0'
            END AS FLAG_TARIKHMATI,
            TO_CHAR(d.TARIKH_MATI,'dd/mm/yyyy') as TARIKH_MATI,
            d.NO_SIJIL_MATI,

  ------ /* Maklumat Pemohon */ ------------
            Upper(REPLACE(e.NAMA_PEMOHON,'&','&#38;')) as NAMA_PEMOHON,
            CASE
                WHEN e.jenis_kp in (0) and e.id_tarafkptg in (1) and nama_pemohon like ('%MAJLIS%AGAMA%ISLAM%') THEN ', No. DAFTAR: '
                WHEN e.id_tarafkptg in (6,10) and nama_pemohon like ('%AMANAH%') THEN null
	     --WHEN e.id_tarafkptg in (6,10) and nama_pemohon like ('%AMANAH%') THEN ', No. DAFTAR: '
                WHEN e.id_tarafkptg in (8) and nama_pemohon like ('%MAJLIS%AGAMA%ISLAM%') THEN ', No. DAFTAR: '
                WHEN e.id_tarafkptg in (6,8) THEN ', No. DAFTAR: '
	      WHEN e.id_tarafkptg in (8) THEN ', No. DAFTAR: '
                WHEN e.jenis_kp in (0) and e.id_tarafkptg=1 THEN ', No. KP: '
                WHEN e.jenis_kp in (0,5,6) THEN ', No. KP: '
                WHEN e.jenis_kp in (7) and e.id_tarafkptg in (1) THEN ', No. KP: '
                WHEN e.jenis_kp=4 AND e.no_kp_baru IS NULL AND e.no_kp_lama IS NULL THEN ', No. PASPORT: '
                WHEN e.id_tarafkptg=1 THEN ', No. KP: '
            -- null,6,0,5,7,4,
                Else ''
            END AS Jenis_kp_pemohon,
            CASE
                WHEN e.no_kp_baru IS NOT NULL AND e.no_kp_lama IS NULL and e.no_kp_lain IS NULL THEN  SUBSTR(e.no_kp_baru,1,6) || '-' || SUBSTR(e.no_kp_baru,7,2) || '-' || SUBSTR(e.no_kp_baru,9,4)
		WHEN e.no_kp_baru IS NOT NULL AND e.no_kp_lama IS NULL and e.no_kp_lain IS NOT NULL THEN  SUBSTR(e.no_kp_baru,1,6) || '-' || SUBSTR(e.no_kp_baru,7,2) || '-' || SUBSTR(e.no_kp_baru,9,4) || ' ('||e.no_kp_lain ||')'
                WHEN e.no_kp_baru IS NOT NULL AND e.no_kp_lama IS NOT NULL AND e.no_kp_lain IS NULL THEN  SUBSTR(e.no_kp_baru,1,6) || '-' || SUBSTR(e.no_kp_baru,7,2) || '-' || SUBSTR(e.no_kp_baru,9,4) || ' ('||e.no_kp_lama ||')'
                WHEN e.no_kp_baru IS NOT NULL AND e.no_kp_lama IS NOT NULL AND e.no_kp_lain IS NOT NULL THEN SUBSTR(e.no_kp_baru,1,6) || '-' || SUBSTR(e.no_kp_baru,7,2) || '-' || SUBSTR(e.no_kp_baru,9,4) || ' ('||e.no_kp_lama ||' / '||e.no_kp_lain ||')'
                WHEN e.no_kp_baru IS NULL AND e.no_kp_lama IS NOT NULL and e.no_kp_lain IS NULL THEN REPLACE(e.no_kp_lama,'&','&#38;')
                WHEN e.no_kp_baru IS NULL AND e.no_kp_lama IS NOT NULL and e.no_kp_lain IS NOT NULL THEN REPLACE(e.no_kp_lama,'&','&#38;') || ' ('||e.no_kp_lain ||')'
                WHEN e.no_kp_baru IS NULL AND e.no_kp_lama IS NULL and e.no_kp_lain IS NOT NULL THEN REPLACE(e.no_kp_lain,'&','&#38;')
                ELSE ''
            END AS kp_pemohon,
            e.JENIS_WARGA AS WARGA_PEMOHON,
            CASE
                WHEN e.ALAMAT_1 IS NULL THEN 'TIADA'
                WHEN e.ALAMAT_2 IS NULL AND e.ALAMAT_3 IS NULL THEN  REPLACE(REPLACE(UPPER(trim(e.ALAMAT_1)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(e.POSKOD),','),'&','&#38;') || ' ' || REPLACE(REPLACE(UPPER(trim(BANDAR.KETERANGAN)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPEMOHON.NAMA_NEGERI)),','),'&','&#38;')
                WHEN e.ALAMAT_2 IS NULL THEN REPLACE(REPLACE(UPPER(trim(e.ALAMAT_1)),','),'&','&#38;') ||', ' || REPLACE(REPLACE(UPPER(trim(e.ALAMAT_3)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(e.POSKOD)),','),'&','&#38;') || ' ' ||REPLACE(REPLACE(UPPER(trim(BANDAR.KETERANGAN)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPEMOHON.NAMA_NEGERI)),','),'&','&#38;')
                WHEN e.ALAMAT_3 IS NULL THEN REPLACE(REPLACE(UPPER(trim(e.ALAMAT_1)),','),'&','&#38;') ||', ' || REPLACE(REPLACE(UPPER(trim(e.ALAMAT_2)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(e.POSKOD)),','),'&','&#38;') || ' ' ||REPLACE(REPLACE(UPPER(trim(BANDAR.KETERANGAN)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPEMOHON.NAMA_NEGERI)),','),'&','&#38;')
                WHEN e.ALAMAT_3 IS NOT NULL THEN REPLACE(REPLACE(UPPER(trim(e.ALAMAT_1)),','),'&','&#38;')||', ' || REPLACE(REPLACE(UPPER(trim(e.ALAMAT_2)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(e.ALAMAT_3)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(e.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(UPPER(trim(BANDAR.KETERANGAN)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPEMOHON.NAMA_NEGERI)),','),'&',chr(38))
                ELSE 'TIADA'
            END AS ALAMAT_PENUH_PEMOHON,

            g.ID_KEPUTUSANPERMOHONAN,

   ------ /* Maklumat Perbicaraan */ ------------
            f.ID_PERBICARAAN,
            f.BIL_BICARA,
             CASE
                WHEN f.ALAMAT_BICARA1 IS NULL AND f.ID_NEGERIBICARA <> 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA2 IS NULL AND f.ALAMAT_BICARA3 IS NULL AND f.ID_NEGERIBICARA <> 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPERBICARAAN.NAMA_NEGERI)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA2 IS NULL AND f.ID_NEGERIBICARA <> 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;') ||', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA3)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;') || ' ' ||REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPERBICARAAN.NAMA_NEGERI)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA3 IS NULL AND f.ID_NEGERIBICARA <> 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;') ||', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA2)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;') || ' ' ||REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPERBICARAAN.NAMA_NEGERI)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA3 IS NOT NULL AND f.ID_NEGERIBICARA <> 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;')||', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA2)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA3)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;') ||', '|| REPLACE(REPLACE(UPPER(trim(NEGERIPERBICARAAN.NAMA_NEGERI)),','),'&','&#38;')

                WHEN f.ALAMAT_BICARA1 IS NULL AND f.ID_NEGERIBICARA = 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA2 IS NULL AND f.ALAMAT_BICARA3 IS NULL AND f.ID_NEGERIBICARA = 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA2 IS NULL AND f.ID_NEGERIBICARA = 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;') ||', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA3)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;') || ' ' ||REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA3 IS NULL AND f.ID_NEGERIBICARA = 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;') ||', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA2)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;') || ' ' ||REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;')
                WHEN f.ALAMAT_BICARA3 IS NOT NULL AND f.ID_NEGERIBICARA = 14 THEN REPLACE(REPLACE(UPPER(trim(f.TEMPAT_BICARA)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA1)),','),'&','&#38;')||', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA2)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.ALAMAT_BICARA3)),','),'&','&#38;') || ', ' || REPLACE(REPLACE(UPPER(trim(f.POSKOD)),','),'&','&#38;')|| ' ' || REPLACE(REPLACE(UPPER(trim(f.BANDAR)),','),'&','&#38;')

                ELSE 'TIADA'
            END AS ALAMAT_PENUH_BICARA,
            TO_CHAR(f.TARIKH_BICARA, 'DD') AS HARI_BICARA,
            CASE
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '01' THEN 'Januari'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '02' THEN 'Februari'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '03' THEN 'Mac'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '04' THEN 'April'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '05' THEN 'Mei'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '06' THEN 'Jun'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '07' THEN 'Julai'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '08' THEN 'Ogos'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '09' THEN 'September'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '10' THEN 'Oktober'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '11' THEN 'November'
                WHEN TO_CHAR (f.TARIKH_BICARA, 'MM') = '12' THEN 'Disember'
                ELSE 'TIADA'
            END AS BULAN_BICARA,
            TO_CHAR(f.TARIKH_BICARA, 'YYYY') AS TAHUN_BICARA,
            TO_CHAR (f.TARIKH_BICARA, 'MM') AS NOBULAN_BICARA,


   ------ /* Maklumat Perintah */ ------------

            h.ID_PERINTAH,
            TO_CHAR(h.TARIKH_PERINTAH, 'DD') AS HARI_PERINTAH,
            CASE
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '01' THEN 'Januari'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '02' THEN 'Februari'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '03' THEN 'Mac'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '04' THEN 'April'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '05' THEN 'Mei'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '06' THEN 'Jun'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '07' THEN 'Julai'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '08' THEN 'Ogos'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '09' THEN 'September'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '10' THEN 'Oktober'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '11' THEN 'November'
                WHEN TO_CHAR (h.TARIKH_PERINTAH, 'MM') = '12' THEN 'Disember'
                ELSE 'TIADA'
            END AS BULAN_PERINTAH,
            TO_CHAR(h.TARIKH_PERINTAH, 'YYYY') AS TAHUN_PERINTAH,
            TO_CHAR(h.TARIKH_PERINTAH, 'DD/MM/YYYY') AS TARIKH_PERINTAH,
            (SELECT DISTINCT REPLACE(TO_CHAR(JUMLAH_BAYARAN,'999,999,999.99'),' ') FROM TBLPPKBAYARAN WHERE ID_PERMOHONAN = b.ID_PERMOHONAN AND ID_JENISBAYARAN = 26) AS CUKAI_PUSAKA,
            (SELECT DISTINCT JUMLAH_BAYARAN FROM TBLPPKBAYARAN WHERE ID_PERMOHONAN = b.ID_PERMOHONAN AND ID_JENISBAYARAN = 26) AS CUKAI_PUSAKA_1,
            (SELECT DISTINCT COUNT(*) FROM TBLPPKPERINTAHHTAOBMST WHERE ID_PERINTAH = h.ID_PERINTAH AND ID_JENISPERINTAH IN (1,7,9)) AS ID_PERINTAHHTAOBMST,
            (SELECT DISTINCT COUNT(*) FROM TBLPPKPERINTAHHAOBMST WHERE ID_PERINTAH = h.ID_PERINTAH AND ID_JENISPERINTAH IN (1,7,9)) AS ID_PERINTAHHAOBMST,
            HIS.NO_PINDAAN, TO_CHAR(HIS.TARIKH_KEMASKINI,'DD/MM/YYYY') AS TARIKH_PINDAAN
From        Tblpfdfail A,
            Tblppkpermohonan B,
            Tblppkpermohonansimati C,
            Tblppksimati D,
            Tblppkpemohon E,
            Tblppkperbicaraan F,
            Tblppkkeputusanpermohonan G,
            Tblppkperintah H,
            Tblrujnegeri Nmhn,
            TBLRUJDAERAH dmhn,
            Tblrujbandar Bandar,
            Tblrujnegeri Negeripemohon,
            Tblrujnegeri Negeriperbicaraan,
