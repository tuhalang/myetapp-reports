	<field name="ID_PERINTAHHTAOBDTL" class="java.lang.String"/>
	<field name="NO_KP" class="java.lang.String"/>
	<field name="WARGANEGARA" class="java.lang.String"/>
	<field name="SINGLEWARIS" class="java.lang.Integer"/>
