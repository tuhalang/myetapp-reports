				<textElement>
					<font fontName="Verdana" size="8"/>
				</textElement>
				<textFieldExpression><![CDATA[($F{NAMA_OB} != null ? $F{NAMA_OB}:"TIADA")+ ($F{NO_KP} != null ? $F{NO_KP}:"")+($F{WARGANEGARA} != null? "\nWarganegara : "+$F{WARGANEGARA}: "")]]></textFieldExpression>
			</textField>
