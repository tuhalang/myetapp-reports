+ ($F{JENIS_WARGA}.equals("1") || $F{JENIS_WARGA}.equals("3") ?($F{NO_KP_BARU}!= null || $F{NO_KP_LAMA} != null || $F{NO_KP_LAIN} != null?"\nNo. KP: "+ $F{NO_KP} :" "):"\nNo. Pasport: "+$F{NO_KP}) :"TIADA")
+($F{ID_TARAFKPTG}.equals("6") ?" ":"\nWarganegara :"+$F{WARGANEGARA}))+($F{STATUS_TADBIR}=="Y"?"\nadalah pentadbir kepada:":"\nadalah pemegang amanah kepada:")]]></textFieldExpression>
