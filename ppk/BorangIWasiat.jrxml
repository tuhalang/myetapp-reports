	<field name="TBLPPKKEPUTUSANPERMOHONAN_KEPU" class="java.lang.String"/>
	<field name="NEGERI_MAHKAMAH_TINGGI" class="java.lang.String"/>
	<field name="DAERAH_MAHKAMAH_TINGGI" class="java.lang.String"/>
