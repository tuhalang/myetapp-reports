	<parameter name="NILAI_BERSIH_2" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT P.ID_PERMOHONAN,
(P.NILAI_TARIKHMOHON - NVL(SI.NILAI_HUTANG,0)) NILAI_TARIKHMOHON,
	CASE
	  WHEN LENGTH(REPLACE(SUBSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),INSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),'.'),LENGTH((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0))),'.')) =  LENGTH((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)) THEN '00'
	  WHEN LENGTH(REPLACE(SUBSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),INSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),'.'),LENGTH((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0))),'.')) = 1 THEN REPLACE(SUBSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),INSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),'.'),LENGTH(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)))),'.') || '0'
	  ELSE REPLACE(SUBSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),INSTR(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)),'.'),LENGTH(((P.NILAI_TARIKHMOHON) - NVL(SI.NILAI_HUTANG,0)))),'.')
	END AS
NILAI_TARIKHMOHON2
FROM (
	SELECT SUM(NILAI_TARIKHMOHON) NILAI_TARIKHMOHON,ID_FAIl,ID_PERMOHONAN  FROM (
		SELECT * FROM VPPK_HAPERINTAH
		UNION
		SELECT * FROM VPPK_HTAPERINTAH
	) GROUP BY ID_FAIL,ID_PERMOHONAN
) P
,(SELECT NVL(OB.NILAI_HUTANG,0) NILAI_HUTANG,SI.ID_PERMOHONAN
	FROM TBLPPKOB OB,TBLPPKPERMOHONANSIMATI SI
	WHERE OB.ID_PERMOHONANSIMATI= SI.ID_PERMOHONANSIMATI
	AND OB.ID_TARAFKPTG = 2
) SI
WHERE
P.ID_PERMOHONAN = SI.ID_PERMOHONAN(+)
AND P.ID_FAIL = $P{idfail}
--AND P.ID_FAIL = 99201193597]]>
	</queryString>
	<field name="ID_PERMOHONAN" class="java.math.BigDecimal"/>
	<field name="NILAI_TARIKHMOHON" class="java.math.BigDecimal"/>
	<field name="NILAI_TARIKHMOHON2" class="java.lang.String"/>
