				<reportElement uuid="757b2715-70f7-4214-a2ce-9f15d8fb7d78" key="textField-19" positionType="Float" x="0" y="709" width="535" height="13" isRemoveLineWhenBlank="true"/>
				<textElement textAlignment="Center" markup="styled">
					<font fontName="Verdana" size="8" isBold="true" pdfFontName="Helvetica"/>
				</textElement>
				<textFieldExpression><![CDATA[($F{POSKOD}!= null ? $F{POSKOD}:"")+" "+($F{BANDAR_UNIT}!= null ? $F{BANDAR_UNIT}:"")+", "+"<style isBold='true' isUnderline='true' pdfFontName='Helvetica-Bold'>"+($F{NEGERI_UNIT}!= null ? $F{NEGERI_UNIT}:" ")+"</style>"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement uuid="43c9632f-b03d-4b80-b541-8b97c6b02e4f" key="textField-20" positionType="Float" x="0" y="723" width="535" height="10" isRemoveLineWhenBlank="true"/>
				<textElement textAlignment="Center" verticalAlignment="Top" markup="styled">
