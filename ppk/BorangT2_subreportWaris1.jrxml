			<textField isStretchWithOverflow="true" pattern="##0.00" isBlankWhenNull="true">
				<reportElement uuid="f93fb43d-30fd-4785-b11f-af6f2d40d124" key="textField" positionType="Float" x="163" y="1" width="70" height="12" isRemoveLineWhenBlank="true"/>
				<textElement textAlignment="Center">
					<font fontName="Verdana" size="9"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{BA_WARIS}+" / "+$F{BB_WARIS}+" bhg"]]></textFieldExpression>
			</textField>
