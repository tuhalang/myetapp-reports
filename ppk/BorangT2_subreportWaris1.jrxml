	<field name="ID_TARAFKPTG" class="java.lang.String"/>
	<field name="ID_PA4" class="java.math.BigDecimal"/>
	<field name="NAMA_OB" class="java.lang.String"/>
	<field name="NO_KP_BARU" class="java.lang.String"/>
