<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="NotaPerbicaraan" pageWidth="595" pageHeight="842" whenNoDataType="BlankPage" columnWidth="509" leftMargin="56" rightMargin="30" topMargin="28" bottomMargin="28" uuid="a9e4cd48-8a32-4573-8f63-2d6808935edc">
