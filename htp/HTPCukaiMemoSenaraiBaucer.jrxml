	<queryString><![CDATA[select trd.ID_DAERAH, trd.NAMA_DAERAH, count(ct.ID_DAERAH) as sumIdDaerah, sum(ct.CUKAI_KENA_BAYAR) as sumKenaBayar,
sum(ct.TUNGGAKAN) as sumtungakan, sum(ct.DENDA) as sumDenda, sum(ct.BAYARAN_LAIN) as sumLain2, sum(ct.CUKAI_PERLU_BAYAR) as sumPerluBayar, 
sum(ct.PENGURANGAN) as sumPengurangan, sum(ct.PENGECUALIAN) as sumPengecualian
,RN.NAMA_NEGERI,
	(SELECT BC.NO_RUJBAYARAN FROM TBLHTPBAUCER B,TBLHTPBAYARANCUKAI BC 
	WHERE ID_DAERAH=trd.ID_DAERAH
	AND BC.ID_BAUCER=B.ID_BAUCER
) NO_BAUCER
,(SELECT BC.TARIKH_BAYARAN FROM TBLHTPBAUCER B,TBLHTPBAYARANCUKAI BC 
	WHERE ID_DAERAH=trd.ID_DAERAH
	AND BC.ID_BAUCER=B.ID_BAUCER
) TARIKH_BAYARAN
from TBLHTPCUKAITEMP ct, Tblrujdaerah trd, TBLRUJNEGERI RN
where ct.ID_DAERAH = trd.ID_DAERAH
AND RN.ID_NEGERI=TRD.ID_NEGERI
and ct.ID_NEGERI = $P!{ID_NEGERI}
group by trd.ID_DAERAH, trd.NAMA_DAERAH,RN.NAMA_NEGERI]]></queryString>

