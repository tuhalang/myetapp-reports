	<parameter name="ID_AGENSI" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT DISTINCT RN.ID_NEGERI,RN.NAMA_NEGERI
,(SELECT COUNT(*)
	FROM 
	TBLRUJNEGERI C,TBLRUJKEMENTERIAN RK,TBLHTPHAKMILIK D,TBLRUJAGENSI RA
	WHERE D.ID_KEMENTERIAN = RK.ID_KEMENTERIAN
    	AND D.ID_AGENSI = RA.ID_AGENSI	
	AND C.ID_NEGERI = D.ID_NEGERI 
	AND D.ID_AGENSI = $P!{ID_AGENSI}
	AND D.ID_NEGERI = RN.ID_NEGERI
	AND NVL(D.NO_WARTA,' ')<>' '  
	AND D.STATUS_SAH IN 
	(
		SELECT STATUS_SAH FROM TBLHTPRUJSTATUSAH WHERE AKTIF=1
	)
) AS BIL_LOT_NEGERI
,(SELECT NVL(SUM(D.LUAS_BERSAMAAN),0)
	FROM TBLRUJNEGERI C,TBLRUJKEMENTERIAN RK,TBLHTPHAKMILIK D,TBLRUJAGENSI RA
	WHERE D.ID_KEMENTERIAN = RK.ID_KEMENTERIAN
    	AND D.ID_AGENSI = RA.ID_AGENSI	
	AND C.ID_NEGERI = D.ID_NEGERI 
	AND D.ID_AGENSI = $P!{ID_AGENSI}
	AND D.ID_NEGERI = RN.ID_NEGERI
	AND NVL(D.NO_WARTA,' ')<>' ' 
	AND D.STATUS_SAH IN 
	(
    		SELECT STATUS_SAH FROM TBLHTPRUJSTATUSAH WHERE AKTIF=1
	)
) AS TOTAL_LUAS_NEGERI
,(
	SELECT NAMA_AGENSI FROM TBLRUJAGENSI
	WHERE 
	ID_AGENSI = $P!{ID_AGENSI}
)AGENSI
FROM TBLRUJNEGERI RN
WHERE RN.ID_NEGERI NOT IN (0,17)
ORDER BY RN.KOD_MAMPU]]></queryString>

