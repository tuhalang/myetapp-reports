	<parameter name="ID_KEMENTERIAN" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT distinct(RA.ID_AGENSI),RA.NAMA_AGENSI
,(SELECT COUNT(*)
    FROM 
    TBLRUJKEMENTERIANMAPPING C,
    TBLHTPHAKMILIK D 
    WHERE C.ID_KEMENTERIANLAMA = D.ID_KEMENTERIAN 
    AND C.ID_KEMENTERIANBARU = RA.ID_KEMENTERIAN
    AND NVL(D.NO_WARTA,' ')<>' ' 
    AND D.STATUS_SAH NOT IN ('B','P','S','M')
    AND D.ID_AGENSI = RA.ID_AGENSI
) AS BIL_LOT_AGENSI
,(SELECT NVL(SUM(D.LUAS_BERSAMAAN),0)
    FROM TBLRUJKEMENTERIANMAPPING C,TBLHTPHAKMILIK D
    WHERE C.ID_KEMENTERIANLAMA = D.ID_KEMENTERIAN 
    AND C.ID_KEMENTERIANBARU = RA.ID_KEMENTERIAN
    AND NVL(D.NO_WARTA,' ')<>' '
    AND D.STATUS_SAH NOT IN ('B','P','S','M')
    AND D.ID_AGENSI = RA.ID_AGENSI
) AS TOTAL_LUAS_AGENSI
,( SELECT NAMA_KEMENTERIAN FROM TBLRUJKEMENTERIAN WHERE ID_KEMENTERIAN = RA.ID_KEMENTERIAN
) NAMA_KEMENTERIAN
FROM 
TBLRUJAGENSI RA
WHERE 
--RA.ID_KEMENTERIAN = 14
RA.ID_KEMENTERIAN = $P{ID_KEMENTERIAN}
ORDER BY RA.NAMA_AGENSI]]></queryString>

