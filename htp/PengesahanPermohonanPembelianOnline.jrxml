from tblpermohonan P
--,  tblHTPpermohonan PP,tblrujnegeri RN, tblrujdaerah RD
,tblpfdfail F
where P.ID_FAIL = F.ID_FAIL
AND P.ID_PERMOHONAN = $P!{ID_PERMOHONAN}]]>
