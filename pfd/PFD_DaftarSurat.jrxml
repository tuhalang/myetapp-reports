	<queryString>
		<![CDATA[SELECT DISTINCT a.ID_LOGDOKUMEN , a.PENGIRIM_MASUK, a.PENERIMA_KELUAR, UPPER(a.NO_RUJUKAN) NO_RUJUKAN , a.TAJUK_DOKUMEN, a.TARIKH_DOKUMEN, a.TARIKH_DOKUMENDITERIMA, a.FLAG_LOGDOKUMEN, a.STATUS_LOGDOKUMEN
FROM
	TBLPFDLOGDOKUMEN a
where
to_char(TARIKH_DOKUMENDITERIMA,'dd/mm/yyyy')  >= '$P!{dari_tahun}'
and
to_char(TARIKH_DOKUMENDITERIMA,'dd/mm/yyyy')  <= '$P!{hingga_tahun}'
and
id_seksyen =(select id_seksyen from users_internal where user_id='$P!{user_id}')
and
id_negeri =(select id_negeri from users_internal where user_id='$P!{user_id}')
AND a.FLAG_LOGDOKUMEN = '$P!{Flag_dokumen}'
ORDER BY a.ID_LOGDOKUMEN DESC]]>
	</queryString>
