	<parameter name="tarikh_hingga" isForPrompting="true" class="java.lang.String"/>
	<parameter name="noMel" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT   B.NO_DAFTAR_MEL, B.TARIKH, C.NAMA, B.NO_RUJUKAN, B.TARIKH_CEK,
         E.NAMA_BANK, B.KREDIT, B.NO_RESIT, B.TARIKH_RESIT
    FROM TBLPHPHASIL A, TBLPHPAKAUN B, TBLPHPPEMOHON C, TBLRUJBANK E
   WHERE A.ID_HASIL = B.ID_HASIL(+)
     AND C.ID_PEMOHON = A.ID_PEMOHON(+)
     AND B.ID_BANK = E.ID_BANK(+)
     AND B.ID_JENISBAYARAN IN (9, 10, 37)
     AND B.ID_CARABAYAR IN (1, 3, 4, 5, 9, 11)
     AND B.FLAG_AKTIF = 'Y'
     AND TO_DATE (B.TARIKH) BETWEEN TO_DATE($P{tarikh_dari}, 'dd/mm/yyyy') AND TO_DATE($P{tarikh_hingga},'dd/mm/yyyy')
     AND NVL (B.NO_DAFTAR_MEL, ' ') LIKE '%' || $P{noMel} || '%'
ORDER BY TO_DATE (B.TARIKH), B.TARIKH_CEK, C.NAMA ASC]]></queryString>

