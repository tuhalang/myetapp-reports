WHERE A.ID_HASIL = B.ID_HASIL(+)
AND C.ID_PEMOHON = A.ID_PEMOHON(+)
AND B.ID_BANK = E.ID_BANK(+)
AND B.ID_CARABAYAR IN(2,5,6)
AND TO_DATE(B.TARIKH) BETWEEN TO_DATE('$P!{tarikh_dari}', 'dd/mm/yyyy') AND TO_DATE('$P!{tarikh_hingga}','dd/mm/yyyy')
