			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="3f6d4e12-8137-4593-a43f-4763ab67972e" key="textField-18" positionType="Float" x="63" y="68" width="388" height="16"/>
