	<queryString>
		<![CDATA[SELECT DISTINCT A.ID_FAIL,A.NO_FAIL,A.TAJUK_FAIL, INITCAP(C.NAMA) AS NAMA_PEMEGANG_LESEN,
INITCAP(C.ALAMAT1_TETAP) AS ALAMAT_PEMOHON1,INITCAP(C.ALAMAT2_TETAP) AS ALAMAT_PEMOHON2, INITCAP(C.ALAMAT3_TETAP) AS ALAMAT_PEMOHON3, C.NO_PENGENALAN,
NVL(REPLACE(TO_CHAR(G.JUMLAH_FEELESEN,'999,999,999.99'),' ', ''),' ') AS JUMLAH_FEELESEN, G.NO_LESEN,
TO_CHAR(G.TARIKH_MULA_LESEN,'DD/MM/YYYY') AS TARIKH_MULA_LESEN,
TO_CHAR(G.TARIKH_TAMAT_LESEN, 'DD/MM/YYYY') AS TARIKH_TAMAT_LESEN,
G.LUAS_KAWASAN, INITCAP(E.LOKASI_PERMOHONAN) AS TEMPAT,
TO_CHAR(I.TARIKH_KELUAR_LESEN,'DD/MM/YYYY') AS TARIKH_KELUAR_LESEN
,H.ID_JENISBAYARAN
--,H.NO_RESIT

FROM TBLPFDFAIL A,TBLPERMOHONAN B, TBLPHPPEMOHON C,TBLPHPPMOHONNJDUALPERTAMA E,TBLPHPBYRNSYRTKLLSNLESENAPB G, TBLPHPJADUALKEDUALESENAPB I,TBLPHPAKAUN H

WHERE A.ID_FAIL=B.ID_FAIL
AND C.ID_PEMOHON=B.ID_PEMOHON
AND G.ID_PERMOHONAN=B.ID_PERMOHONAN
AND G.ID_PERMOHONAN=E.ID_PERMOHONAN
AND E.ID_PHPPMOHONNJDUALPERTAMA = H.ID_PHPPMOHONNJDUALPERTAMA
AND H.ID_JENISBAYARAN IN (19)
--AND H.NO_RESIT IS NOT NULL
--AND G.TARIKH_MULA_LESEN IS NOT NULL
AND A.ID_FAIL = $P{ID_FAIL}]]>
	</queryString>
