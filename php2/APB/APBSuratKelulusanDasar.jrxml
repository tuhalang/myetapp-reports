				<textFieldExpression><![CDATA["\n\nPengerusi\n"
+""+($F{NAMA}!=null && $F{NAMA}!=""?$F{NAMA}+"\n":"")
+""+($F{ALAMAT_1}!=null && $F{ALAMAT_1}!=""?$F{ALAMAT_1}+"\n":"")
+""+($F{ALAMAT_2}!=null && $F{ALAMAT_2}!=""?$F{ALAMAT_2}+"\n":"")
