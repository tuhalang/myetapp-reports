	<queryString><![CDATA[SELECT
TO_CHAR(D.TARIKH_HANTAR, 'DD/') AS HARI_HANTAR, D.HARI_HANTAR AS HARI,
    CASE
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '01' THEN '01'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '02' THEN '02'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '03' THEN '03'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '04' THEN '04'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '05' THEN '05'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '06' THEN '06'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '07' THEN '07'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '08' THEN '08'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '09' THEN '09'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '10' THEN '10'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '11' THEN '11'
       WHEN TO_CHAR (D.TARIKH_HANTAR, 'MM') = '12' THEN '12'
       ELSE 'TIADA'
    END AS BULAN_HANTAR,
CASE
     WHEN D.MINIT_HANTAR = '0' THEN '00'
     WHEN D.MINIT_HANTAR = '1' THEN '05'
     WHEN D.MINIT_HANTAR = '2' THEN '10'
     WHEN D.MINIT_HANTAR = '3' THEN '15'
     WHEN D.MINIT_HANTAR = '4' THEN '20'
     WHEN D.MINIT_HANTAR = '5' THEN '25'
     WHEN D.MINIT_HANTAR = '6' THEN '30'
     WHEN D.MINIT_HANTAR = '7' THEN '35'
     WHEN D.MINIT_HANTAR = '8' THEN '40'
     WHEN D.MINIT_HANTAR = '9' THEN '45'
     WHEN D.MINIT_HANTAR = '10' THEN '50'
     WHEN D.MINIT_HANTAR = '11' THEN '55' ELSE D.MINIT_HANTAR END AS MINIT_HANTAR,
CASE
     WHEN D.JAM_HANTAR = '0' THEN '00'
     WHEN D.JAM_HANTAR = '1' THEN '01'
     WHEN D.JAM_HANTAR = '2' THEN '02'
     WHEN D.JAM_HANTAR = '3' THEN '03'
     WHEN D.JAM_HANTAR = '4' THEN '04'
     WHEN D.JAM_HANTAR = '5' THEN '05'
     WHEN D.JAM_HANTAR = '6' THEN '06'
     WHEN D.JAM_HANTAR = '7' THEN '07'
     WHEN D.JAM_HANTAR = '8' THEN '08'
     WHEN D.JAM_HANTAR = '9' THEN '09' ELSE D.JAM_HANTAR END AS JAM_HANTAR,
    TO_CHAR(D.TARIKH_HANTAR, '/YYYY') AS TAHUN_HANTAR,
D.NAMA_BARGE,D.LOKASI_DIBEKALKAN,D.AKUAN_KASTAM,D.NO_KASTAM,
REPLACE(TO_CHAR(D.KUANTITI,'999,999,999.99'),' ') AS KUANTITI,
REPLACE(TO_CHAR(D.ANGGARAN_ROYALTI,'999,999,999.99'),' ') AS ANGGARAN_ROYALTI
FROM TBLPHPJADUALKEDUALESENAPB A,
TBLPHPPEMEGANG B,
TBLPHPLAPORANPASIR C,
TBLPHPBORANGK2K3 D
WHERE A.ID_JADUALKEDUALESENAPB = B.ID_JADUALKEDUALESENAPB
AND B.ID_JADUALKEDUALESENAPB = C.ID_JADUALKEDUALESENAPB
AND C.ID_LAPORANPASIR = D.ID_LAPORANPASIR
AND A.ID_JADUALKEDUALESENAPB = $P!{id_jadualkedualesenapb}
AND C.BULAN_PENGAMBILAN = $P!{bulan_pengambilan}
AND C.ID_LAPORANPASIR =  $P!{id_laporanpasir}]]></queryString>

