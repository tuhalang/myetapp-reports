				<textElement textAlignment="Right" verticalAlignment="Top">
					<font fontName="Arial" size="11"/>
				</textElement>
				<textFieldExpression><![CDATA["Tarikh : "]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="false">
