<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="APBPengesahanPelanKawasan" pageWidth="600" pageHeight="842" columnWidth="456" leftMargin="72" rightMargin="72" topMargin="144" bottomMargin="72" uuid="a299c975-b406-49ca-8e3e-2bffa755e4f7">
