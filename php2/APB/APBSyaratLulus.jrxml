    WHEN TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAS, 'MM')= 10 THEN 'Oktober' || ' ' || TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAS, 'YYYY')
    WHEN TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAS, 'MM')= 11 THEN 'November' || ' ' || TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAS, 'YYYY')
