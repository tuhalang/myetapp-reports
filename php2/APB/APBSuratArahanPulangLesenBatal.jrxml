	<parameter name="ID_FAIL" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ID_PERMOHONAN" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ID_RUJUKAN" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="NAMA_PEGAWAI" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	A.ID_FAIL,
	A.TAJUK_FAIL,
	B.ID_PERMOHONAN,
	A.NO_FAIL,
	C.ID_PEMOHON,
	REPLACE(INITCAP(REPLACE(TRIM(C.NAMA), '&', '&#38;')), ',') AS NAMA,
	REPLACE(INITCAP(REPLACE(TRIM(C.ALAMAT1_TETAP), '&', '&#38;')), ',') AS ALAMAT1,
	REPLACE(INITCAP(REPLACE(TRIM(C.ALAMAT2_TETAP), '&', '&#38;')), ',') AS ALAMAT2,
	REPLACE(INITCAP(REPLACE(TRIM(C.ALAMAT3_TETAP), '&', '&#38;')), ',') AS ALAMAT3,
	C.POSKOD_TETAP,
	INITCAP(D.NAMA_NEGERI) AS NAMA_NEGERI,
	E.KETERANGAN,
	F.ID_NEGERI_PERAIRAN,
	F.ID_JENIS_LESEN,
	CASE
		WHEN F.ID_JENIS_LESEN = '2' THEN 'BORANG 2'
		WHEN F.ID_JENIS_LESEN = '3' THEN 'BORANG 3'
		WHEN F.ID_JENIS_LESEN = '4' THEN 'BORANG 4'
	END AS JENIS_LESEN,
	CASE
		WHEN F.ID_JENIS_LESEN = '2' THEN ''
		WHEN F.ID_JENIS_LESEN = '3' THEN F.JENIS_GALIAN
		WHEN F.ID_JENIS_LESEN = '4' THEN F.JENIS_GALIAN
	END AS JENIS_GALIAN,
	INITCAP(PERAIRAN.NAMA_NEGERI) AS NEGERI,
	INITCAP(G.KETERANGAN) AS BANDAR,
	H.ID_PERMOHONAN,
	TO_CHAR(H.TARIKH_TAMAT_LESEN, 'DD') AS HARI_SURAT,
	CASE
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 01 THEN 'Januari' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 02 THEN 'Februari' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 03 THEN 'Mac' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 04 THEN 'April' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 05 THEN 'Mei' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 06 THEN 'Jun' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 07 THEN 'Julai' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 08 THEN 'Ogos' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 09 THEN 'September' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 10 THEN 'Oktober' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 11 THEN 'November' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
		WHEN TO_CHAR(H.TARIKH_TAMAT_LESEN, 'MM')= 12 THEN 'Disember' || ' ' || TO_CHAR(H.TARIKH_TAMAT_LESEN, 'YYYY')
	END AS TARIKH_TAMAT_LESEN,
	H.NO_LESEN,
	TO_CHAR(SYSDATE, 'DD') AS HARI,
	CASE
		WHEN TO_CHAR(SYSDATE, 'MM')= 01 THEN 'Januari' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 02 THEN 'Februari' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 03 THEN 'Mac' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 04 THEN 'April' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 05 THEN 'Mei' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 06 THEN 'Jun' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 07 THEN 'Julai' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 08 THEN 'Ogos' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 09 THEN 'September' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 10 THEN 'Oktober' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 11 THEN 'November' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
		WHEN TO_CHAR(SYSDATE, 'MM')= 12 THEN 'Disember' || ' ' || TO_CHAR(SYSDATE, 'YYYY')
	END AS TARIKH_HARI_INI
FROM
	TBLPFDFAIL A,
	TBLPERMOHONAN B,
	TBLPHPPEMOHON C,
	TBLRUJNEGERI D,
	TBLRUJSTATUS E,
	TBLPHPPMOHONNJDUALPERTAMA F,
	TBLRUJNEGERI PERAIRAN,
	TBLRUJBANDAR G,
	TBLPHPBYRNSYRTKLLSNLESENAPB H
--	TBLPHPLESENTAMATTEMPOH I
WHERE
	A.ID_FAIL = B.ID_FAIL
	AND B.ID_PEMOHON = C.ID_PEMOHON
	AND C.ID_NEGERITETAP = D.ID_NEGERI
	AND B.ID_STATUS = E.ID_STATUS
	AND C.ID_BANDARTETAP = G.ID_BANDAR(+)
	AND F.ID_NEGERI_PERAIRAN = PERAIRAN.ID_NEGERI(+)
	AND F.ID_PERMOHONAN = H.ID_PERMOHONAN
	AND A.ID_FAIL = $P!{ID_FAIL}
	AND F.ID_PERMOHONAN = $P!{ID_PERMOHONAN}]]>
	</queryString>
