            WHEN TO_CHAR(TBLPHPBYRNSYRTKLLSNLESENAPB.TARIKH_LULUS_JAS,'MM')=10 THEN 'Oktober' ||' '|| TO_CHAR(TBLPHPBYRNSYRTKLLSNLESENAPB.TARIKH_LULUS_JAS,'YYYY')
