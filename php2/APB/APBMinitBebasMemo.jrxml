            WHEN TO_CHAR(TBLPHPBYRNSYRTKLLSNLESENAPB.TARIKH_LULUSPHN,'MM')=10 THEN 'Oktober' ||' '|| TO_CHAR(TBLPHPBYRNSYRTKLLSNLESENAPB.TARIKH_LULUSPHN,'YYYY')
