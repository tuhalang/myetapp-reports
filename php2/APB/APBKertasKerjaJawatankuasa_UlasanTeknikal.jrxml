	<parameter name="ID_FAIL" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 1 AS TURUTAN, 'Jabatan Ukur dan Pemetaan Malaysia' AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_JUPEM AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JUPEM, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_JUPEM), '&', '&#38;')),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
UNION
SELECT 2 AS TURUTAN, 'Jabatan Pengairan dan Saliran Malaysia' AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_JPS AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JPS, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_JPS), '&', '&#38;')),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
UNION
SELECT 3 AS TURUTAN, 'Jabatan Mineral dan Geosains Malaysia' AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_JAB_GEOSAINS AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAB_GEOSAINS, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_JAB_GEOSAINS),'&','&#38;')),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
UNION
SELECT 4 AS TURUTAN, 'Jabatan Laut Malaysia' AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_JAB_LAUT AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAB_LAUT, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_JAB_LAUT), '&','&#38;')),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
UNION
SELECT 5 AS TURUTAN, 'Pusat Hidrografi Nasional' AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_PUSAT_HIDROGRAFI AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_PUSAT_HIDROGRAFI, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_PUSAT_HIDROGRAFI),'&','&#38;' )),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
UNION
SELECT 6 AS TURUTAN, 'Jabatan Perikanan Malaysia' AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_JAB_PERIKANAN AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAB_PERIKANAN, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_JAB_PERIKANAN),'&','&#38;' )),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
UNION
SELECT 7 AS TURUTAN, 'Jabatan Alam Sekitar' || ' Negeri ' || INITCAP (TRIM (REPLACE (TBLRUJNEGERI.NAMA_NEGERI, 'NEGERI', ''))) AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_JAS AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_JAS, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_JAS),'&','&#38;' )),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
UNION
SELECT 8 AS TURUTAN, 'Pejabat Pengarah Tanah dan Galian' || ' Negeri ' || INITCAP (TRIM (REPLACE (TBLRUJNEGERI.NAMA_NEGERI, 'NEGERI', ''))) AS AGENSI,
    TBLPHPKERTASKERJAAPB.NO_RUJUKAN_PTG AS NO_RUJUKAN,  TO_CHAR(TBLPHPKERTASKERJAAPB.TARIKH_SURAT_PTG, 'dd/MM/yyyy') AS TARIKH_SURAT,
    REPLACE((REPLACE (TRIM (TBLPHPKERTASKERJAAPB.ULASAN_PTG),'&','&#38;' )),',') AS ULASAN
  FROM TBLPFDFAIL, TBLPERMOHONAN,
       TBLPHPPMOHONNJDUALPERTAMA,
       TBLPHPKERTASKERJAAPB,
       TBLRUJNEGERI
 WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_PERMOHONAN = TBLPERMOHONAN.ID_PERMOHONAN
   AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPKERTASKERJAAPB.ID_PERMOHONAN
   AND TBLPHPPMOHONNJDUALPERTAMA.ID_NEGERI_PERAIRAN = TBLRUJNEGERI.ID_NEGERI(+)
   AND TBLPHPKERTASKERJAAPB.FLAG_KERTAS = 1
   AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}
   ORDER BY TURUTAN ASC]]></queryString>

