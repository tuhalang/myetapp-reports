	<queryString><![CDATA[SELECT 
TBLPFDFAIL.ID_NEGERI,
TBLRUJNEGERI.NAMA_NEGERI,
TBLHTPHAKMILIK.ID_KEMENTERIAN,
TBLPERMOHONAN.ID_PERMOHONAN,
TBLPFDFAIL.ID_FAIL,
INITCAP(TBLPFDFAIL.TAJUK_FAIL) AS TAJUK_FAIL,
INITCAP(TBLRUJKEMENTERIAN.NAMA_KEMENTERIAN) AS PENGGUNA,
INITCAP(TBLPHPPEMOHON.NAMA) AS PEMOHON,
INITCAP(TBLPHPPERMOHONANPELEPASAN.NAMA_PROJEK) AS TUJUAN,
TBLHTPHAKMILIK.LUAS_BERSAMAAN AS LUAS_LOT,
TBLPHPPERMOHONANPELEPASAN.LUAS_MHN AS LUAS_DIPOHON,
TBLPFDFAIL.NO_FAIL, 
TO_CHAR(TBLPFDFAIL.TARIKH_DAFTAR_FAIL,'DD/MM/YYYY') AS TARIKH_DAFTAR_FAIL,
TO_CHAR(TBLPERMOHONAN.TARIKH_TERIMA,'DD/MM/YYYY') AS TARIKH_TERIMA,
CASE
    WHEN TBLPHPPERMOHONANPELEPASAN.FLAG_KEPUTUSAN <> '' AND TBLPHPPERMOHONANPELEPASAN.FLAG_KEPUTUSAN = 'T' THEN 'Surat Tolak dikeluarkan pada ' || TO_CHAR(TBLPHPPERMOHONANPELEPASAN.TARIKH_KEPUTUSAN,'DD/MM/YYYY')
    WHEN TBLPHPPERMOHONANPELEPASAN.FLAG_KEPUTUSAN <> '' AND TBLPHPPERMOHONANPELEPASAN.FLAG_KEPUTUSAN = 'L' THEN 'Surat Kelulusan dikeluarkan pada ' || TO_CHAR(TBLPHPPERMOHONANPELEPASAN.TARIKH_KEPUTUSAN,'DD/MM/YYYY')
    ELSE ''
END AS SURAT_TOLAK_LULUS,
TBLPHPPERMOHONANPELEPASAN.ULASAN_MENTERINRE
FROM TBLPFDFAIL, 
TBLPERMOHONAN,
TBLPHPHAKMILIKPERMOHONAN,
TBLPHPPERMOHONANPELEPASAN,
TBLHTPHAKMILIK,
TBLRUJKEMENTERIAN,
TBLPHPPEMOHON,
TBLRUJNEGERI
WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPHAKMILIKPERMOHONAN.ID_PERMOHONAN
AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPPERMOHONANPELEPASAN.ID_PERMOHONAN
AND TBLPHPHAKMILIKPERMOHONAN.ID_HAKMILIK = TBLHTPHAKMILIK.ID_HAKMILIK
AND TBLHTPHAKMILIK.ID_KEMENTERIAN = TBLRUJKEMENTERIAN.ID_KEMENTERIAN(+)
AND TBLPERMOHONAN.ID_PEMOHON = TBLPHPPEMOHON.ID_PEMOHON
AND TBLRUJNEGERI.ID_NEGERI = $P{ID_NEGERI}
AND TBLPFDFAIL.ID_SEKSYEN = '4'
AND TBLPFDFAIL.ID_SUBURUSAN = '33'
AND TBLPFDFAIL.ID_NEGERI = $P{ID_NEGERI}
AND TBLPERMOHONAN.NO_RAYUAN = 0
AND TO_CHAR(TBLPFDFAIL.TARIKH_DAFTAR_FAIL,'YYYY') = $P{TAHUN}]]></queryString>

