	<parameter name="ID_MESYUARAT" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ReportDir" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT DISTINCT
	A.CATATAN,
	A.FLAG_SYOR,
	C.NAMA AS NAMA_PEMOHON,
	REPLACE(REPLACE(TRIM(C.ALAMAT1_TETAP),'&','&#38;'),',')  AS ALAMAT_1,
	REPLACE(REPLACE(TRIM(C.ALAMAT2_TETAP),'&','&#38;'),',')  AS ALAMAT_2,
	REPLACE(REPLACE(TRIM(C.ALAMAT3_TETAP),'&','&#38;'),',')  AS ALAMAT_3,
	UPPER(R.KETERANGAN) AS BANDAR_PEMOHON, S.NAMA_NEGERI AS NEGERI_PEMOHON,
	C.POSKOD_TETAP,
	D.NO_FAIL,
	CASE WHEN E.FLAG_GUNA = '1' THEN 'SEBAHAGIAN'
		 WHEN E.FLAG_GUNA = '2' THEN 'KESELURUHAN'
	END AS FLAG_GUNA, E.LUAS_MHNBERSAMAAN,
	G.NAMA_SUBSUBURUSAN AS TUJUAN,
	J.NAMA_KEMENTERIAN AS KJP,
	I.NO_LOT, K.KETERANGAN,
	L.NAMA_MUKIM, M.NAMA_DAERAH,
	N.NAMA_NEGERI, I.LUAS,
	CASE WHEN I.LUAS IS NOT NULL THEN O.KETERANGAN 
		 ELSE ''
	END AS UNIT_LUAS,
	P.ULASAN AS ULASAN_JPPH, 
	NVL(P.KADAR_SEWA_BULANAN, 0) AS KADAR_SEWA,
	TO_CHAR (Q.TARIKH_MULA_PERJANJIAN, 'dd/MM/yyyy') AS TARIKH_MULA_PERJANJIAN,
	TO_CHAR (Q.TARIKH_TAMAT_PERJANJIAN, 'dd/MM/yyyy') AS TARIKH_TAMAT_PERJANJIAN,
	T.BIL_MESYUARAT
FROM
	TBLPHPMESYUARATPERMOHONAN A,
	TBLPERMOHONAN B,
	TBLPHPPEMOHON C,
	TBLPFDFAIL D,
	TBLPHPPERMOHONANSEWA E,
	TBLPHPPERMOHONANTUJUAN F,
	TBLRUJSUBSUBURUSAN G, 
	TBLPHPHAKMILIKPERMOHONAN H,
	TBLPHPHAKMILIK I,
	TBLRUJKEMENTERIAN J,
	TBLRUJLOT K,
	TBLRUJMUKIM L,
	TBLRUJDAERAH M,
	TBLRUJNEGERI N,
	TBLRUJLUAS O,
	TBLPHPULASANTEKNIKAL P,
	TBLPHPPERJANJIAN Q,
	TBLRUJBANDAR R,
	TBLRUJNEGERI S,
	TBLPHPMESYUARAT T
WHERE
	A.ID_PERMOHONAN = B.ID_PERMOHONAN
	AND B.ID_PEMOHON = C.ID_PEMOHON 
	AND B.ID_FAIL = D.ID_FAIL
	AND B.ID_PERMOHONAN = E.ID_PERMOHONAN 
	AND E.ID_PHPPERMOHONANSEWA = F.ID_PHPPERMOHONANSEWA(+)
	AND F.ID_JENISTUJUAN = G.ID_SUBSUBURUSAN(+)
	AND B.ID_PERMOHONAN = H.ID_PERMOHONAN
	AND H.ID_HAKMILIKPERMOHONAN = I.ID_HAKMILIKPERMOHONAN
	AND I.ID_KEMENTERIAN = J.ID_KEMENTERIAN(+)
	AND I.ID_LOT = K.ID_LOT(+)
	AND I.ID_MUKIM = L.ID_MUKIM(+)
	AND L.ID_DAERAH = M.ID_DAERAH(+)
	AND M.ID_NEGERI = N.ID_NEGERI(+)
	AND I.ID_LUAS = O.ID_LUAS(+)
	AND B.ID_PERMOHONAN = P.ID_PERMOHONAN
	AND B.ID_PERMOHONAN = Q.ID_PERMOHONAN(+)
	AND C.ID_BANDARTETAP = R.ID_BANDAR(+)
	AND C.ID_NEGERITETAP = S.ID_NEGERI(+)
	AND A.ID_MESYUARAT = T.ID_MESYUARAT
	AND P.FLAG_KJP = 'JPPH'
	AND B.FLAG_PERJANJIAN = 'U'
	AND B.FLAG_AKTIF = 'Y'
	AND A.ID_MESYUARAT = $P!{ID_MESYUARAT}]]></queryString>

