	<field name="LUAS_MHNBERSAMAAN" class="java.lang.String"/>
	<field name="TUJUAN" class="java.lang.String"/>
	<field name="JENIS_LOT" class="java.lang.String"/>
	<field name="NO_LOT" class="java.lang.String"/>
