				</textElement>
				<textFieldExpression><![CDATA["Luas kawasan yang diluluskan adalah <style isBold='true' pdfFontName='Helvetica-Bold'>"+($F{LUAS_MHNBERSAMAAN}!=null && $F{LUAS_MHNBERSAMAAN}!="" ? $F{LUAS_MHNBERSAMAAN}+" "+$F{LUAS}:"")+"</style> sahaja;"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="false">
