					<paragraph lineSpacing="Single"/>
				</textElement>
				<textFieldExpression><![CDATA["Kadar sewa adalah sebanyak <style isBold='true' pdfFontName='Helvetica-Bold'>"+($F{KADAR_SEWA}!=null && $F{KADAR_SEWA}!="" ? $F{KADAR_SEWA}:"")+" sebulan</style>;"]]></textFieldExpression>
