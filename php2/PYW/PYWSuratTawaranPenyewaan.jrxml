	<queryString><![CDATA[SELECT  TBLPFDFAIL.ID_FAIL, TBLPFDFAIL.NO_FAIL, TBLPFDFAIL.TAJUK_FAIL,
        --TO_CHAR(SYSDATE,'DD')||' '||
        CASE
             WHEN TO_CHAR(SYSDATE,'MM')=01 THEN 'Januari' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=02 THEN 'Februari' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=03 THEN 'Mac' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=04 THEN 'April' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=05 THEN 'Mei' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=06 THEN 'Jun' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=07 THEN 'Julai' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=08 THEN 'Ogos' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=09 THEN 'September' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=10 THEN 'Oktober' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=11 THEN 'November' ||' '|| TO_CHAR(SYSDATE,'YYYY')
             WHEN TO_CHAR(SYSDATE,'MM')=12 THEN 'Disember' ||' '|| TO_CHAR(SYSDATE,'YYYY')
        END AS TARIKH_HARI_INI,
	TO_CHAR((SYSDATE + 14), 'DD/MM/YYYY') AS TARIKH_KEMUKAKAN_SURAT,
        TO_CHAR(TBLPHPPERJANJIAN.TARIKH_MULA_PERJANJIAN,'DD/MM/YYYY') AS TARIKH_MULA_PERJANJIAN,
        TO_CHAR(TBLPHPPERJANJIAN.TARIKH_TAMAT_PERJANJIAN,'DD/MM/YYYY') AS TARIKH_TAMAT_PERJANJIAN,
        'RM '||(REPLACE(TO_CHAR(TBLPHPPERJANJIAN.KADAR_SEWA,'999,999,999.99'),' ', '')) AS KADAR_SEWA,
        'RM '||(REPLACE(TO_CHAR(TBLPHPPERJANJIAN.CAGARAN,'999,999,999.99'),' ', '')) AS CAGARAN,
        TO_CHAR(TBLPHPPERMOHONANSEWA.LUAS_MHNBERSAMAAN) AS LUAS_MHNBERSAMAAN, LOWER(TBLRUJLUAS.KETERANGAN) AS LUAS,
        INITCAP(TBLPHPPERMOHONANSEWA.TUJUAN) AS TUJUAN
	,LOWER(TBLPHPPERMOHONANSEWA.TUJUAN) TUJUAN_FORMAT
        	, TBLPHPPERJANJIAN.TEMPOH||' bulan' TEMPOH
        ,REPLACE(INITCAP(REPLACE(TRIM(TBLPHPPEMOHON.NAMA),'&','&#38;')),',') NAMA_PEMOHON,
        REPLACE(INITCAP(REPLACE(TRIM(TBLPHPPEMOHON.ALAMAT1_TETAP),'&','&#38;')),',')  as ALAMAT_1,
        REPLACE(INITCAP(REPLACE(TRIM(TBLPHPPEMOHON.ALAMAT2_TETAP),'&','&#38;')),',')  as ALAMAT_2,
        REPLACE(INITCAP(REPLACE(TRIM(TBLPHPPEMOHON.ALAMAT3_TETAP),'&','&#38;')),',')  as ALAMAT_3,
        TBLPHPPEMOHON.POSKOD_TETAP,INITCAP(BANDARPEMOHON.KETERANGAN) AS BANDAR_PEMOHON,
        INITCAP(NEGERIPEMOHON.NAMA_NEGERI) AS NEGERI_PEMOHON,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJKEMENTERIAN.NAMA_KEMENTERIAN),'&','&#38;')),',') NAMA_KJP,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJKEMENTERIAN.ALAMAT1),'&','&#38;')),',')  as ALAMAT_KJP1,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJKEMENTERIAN.ALAMAT2),'&','&#38;')),',')  as ALAMAT_KJP2,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJKEMENTERIAN.ALAMAT3),'&','&#38;')),',')  as ALAMAT_KJP3,
        TBLRUJKEMENTERIAN.POSKOD AS POSKOD_KJP,
        INITCAP(NEGERIKJP.NAMA_NEGERI) AS NEGERI_KJP,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJPEJABATJKPTG.NAMA_PEJABAT),'&','&#38;')),',') AS NAMA_PEJABAT,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJPEJABATJKPTG.ALAMAT1),'&','&#38;')),',') AS ALAMAT1,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJPEJABATJKPTG.ALAMAT2),'&','&#38;')),',') AS ALAMAT2,
        REPLACE(INITCAP(REPLACE(TRIM(TBLRUJPEJABATJKPTG.ALAMAT3),'&','&#38;')),',') AS ALAMAT3,
        TBLRUJPEJABATJKPTG.POSKOD,
        INITCAP (NEGERIPEJABAT.NAMA_NEGERI) AS NEGERI_PEJABAT
   FROM TBLPFDFAIL, TBLPERMOHONAN, TBLPHPPERJANJIAN, TBLPHPPERMOHONANSEWA, TBLPHPPEMOHON, TBLRUJLUAS,
        TBLRUJBANDAR BANDARPEMOHON, TBLRUJNEGERI NEGERIPEMOHON, TBLRUJKEMENTERIAN, TBLRUJNEGERI NEGERIKJP,
        TBLRUJPEJABATJKPTG, TBLRUJNEGERI NEGERIPEJABAT
  WHERE TBLPFDFAIL.ID_FAIL = TBLPERMOHONAN.ID_FAIL
    AND TBLPERMOHONAN.ID_PEMOHON = TBLPHPPEMOHON.ID_PEMOHON
    AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPPERJANJIAN.ID_PERMOHONAN
    AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPPERMOHONANSEWA.ID_PERMOHONAN
    AND TBLPHPPERMOHONANSEWA.ID_LUASMHNBERSAMAAN = TBLRUJLUAS.ID_LUAS(+)
    AND TBLPHPPEMOHON.ID_BANDARTETAP = BANDARPEMOHON.ID_BANDAR(+)
    AND TBLPHPPEMOHON.ID_NEGERITETAP = NEGERIPEMOHON.ID_NEGERI(+)
    AND TBLPFDFAIL.ID_KEMENTERIAN = TBLRUJKEMENTERIAN.ID_KEMENTERIAN(+)
    AND TBLRUJKEMENTERIAN.ID_NEGERI = NEGERIKJP.ID_NEGERI(+)
    AND TBLPFDFAIL.ID_NEGERI = TBLRUJPEJABATJKPTG.ID_NEGERI(+)
    AND TBLRUJPEJABATJKPTG.ID_NEGERI = NEGERIPEJABAT.ID_NEGERI(+)
    AND TBLRUJPEJABATJKPTG.ID_SEKSYEN = 4
    AND TBLPHPPERJANJIAN.FLAG_PERJANJIAN = 'U'
    AND TBLPFDFAIL.ID_FAIL = $P!{ID_FAIL}]]></queryString>

