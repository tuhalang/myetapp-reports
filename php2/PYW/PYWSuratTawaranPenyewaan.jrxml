				</textElement>
				<textFieldExpression><![CDATA["Penyewaan adalah untuk tujuan <style isBold='true'>"+($F{TUJUAN_FORMAT}!=null ? $F{TUJUAN_FORMAT}:"")+"</style> sahaja;"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="false">
