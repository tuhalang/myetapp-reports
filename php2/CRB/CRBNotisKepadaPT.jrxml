REPLACE(INITCAP(REPLACE(TRIM(TBLPHPPEMOHON.ALAMAT2_TETAP),'&','&#38;')),',') as ALAMAT_2_PEMOHON,
REPLACE(INITCAP(REPLACE(TRIM(TBLPHPPEMOHON.ALAMAT3_TETAP),'&','&#38;')),',') as ALAMAT_3_PEMOHON,
