AND TBLPERMOHONAN.ID_PERMOHONAN = TBLPHPHAKMILIKPERMOHONAN.ID_PERMOHONAN
AND TBLPHPHAKMILIKPERMOHONAN.ID_HAKMILIKPERMOHONAN = TBLPHPHAKMILIK.ID_HAKMILIKPERMOHONAN(+)
AND TBLPFDFAIL.ID_SEKSYEN = 4 AND TBLPFDFAIL.ID_SUBURUSAN = '56' AND TBLPFDFAIL.ID_URUSAN = '8' 
AND TBLPERMOHONAN.ID_STATUS_SEBELUM(+) = TBLRUJSTATUS.ID_STATUS
AND TBLPERMOHONAN.ID_STATUS = 1610212
$P!{TAHUN_1}
GROUP BY TBLRUJSTATUS.ID_STATUS,TBLPERMOHONAN.ID_STATUS_SEBELUM, TBLRUJSTATUS.KETERANGAN
UNION
SELECT (CASE TBLRUJSTATUS.ID_STATUS
WHEN 1610198 THEN 1
