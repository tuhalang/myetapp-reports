	<queryString><![CDATA[----------------------  SURAT IRINGAN NOTIS  ---------------------

SELECT DISTINCT B.ID_FAIL, F2.ID_PEJABATJKPTG,
    CASE 
        WHEN length(rtrim(AAA.NO_KP1)) is null THEN  ''
        WHEN LENGTH(RTRIM(AAA.NO_KP1))<12 THEN  ''||RTRIM(AAA.NO_KP1)||''
        WHEN LENGTH(RTRIM(AAA.NO_KP1))=12 THEN SUBSTR(AAA.NO_KP1,1,6) || '-' || SUBSTR(AAA.NO_KP1,7,2) || '-' || SUBSTR(AAA.NO_KP1,9,4) 
    ELSE SUBSTR(AAA.NO_KP1,1,6) || '-' || SUBSTR(AAA.NO_KP1,7,2) || '-' || SUBSTR(AAA.NO_KP1,9,4)  ||'  ('||TRIM(SUBSTR(AAA.NO_KP1,13,LENGTH(AAA.NO_KP1)))||')'
    END  AS KP_SIMATI,
    UPPER(H.NAMA_SIMATI) AS NAMA_SIMATI, 
    TO_CHAR(H.TARIKH_MATI,'dd/mm/yyyy') AS TARIKH_MATI, 
    CASE
        WHEN LENGTH(b.no_fail)>=30 THEN ''||substr(b.no_fail,1,24)||''
        WHEN LENGTH(b.no_fail)<=25 THEN ''||rtrim(b.no_fail)||''
       END AS no_fail, 
    REPLACE(INITCAP(REPLACE(TRIM(G2.NAMA_PEJABAT),'&','&#38;')),',') AS NAMA_PEJABAT,
    REPLACE(INITCAP(REPLACE(TRIM(G2.ALAMAT1),'&','&#38;')),',') AS ALAMAT1_PEJABAT,
    REPLACE(INITCAP(REPLACE(TRIM(G2.ALAMAT2),'&','&#38;')),',') AS ALAMAT2_PEJABAT,
    REPLACE(INITCAP(REPLACE(TRIM(G2.ALAMAT3),'&','&#38;')),',') AS ALAMAT3_PEJABAT,
    
    CASE
        WHEN G2.POSKOD IS NULL THEN '' ELSE G2.POSKOD
     END AS POSKOD_PEJABAT,
    CASE
        WHEN UPPER(TRIM(H2.KETERANGAN)) <> 'TIADA MAKLUMAT' THEN NVL(UPPER(TRIM(H2.KETERANGAN)),'')
        ELSE ''
    END AS BANDAR_PEJABAT,
    CASE
        WHEN UPPER(TRIM(J2.NAMA_NEGERI)) <> 'TIADA MAKLUMAT' THEN NVL(UPPER(TRIM(J2.NAMA_NEGERI)),'')
        ELSE ''
    END AS NAMA_NEGERI_PEJABAT, 
    CASE
        WHEN UPPER(TRIM(I2.NAMA_DAERAH)) <> 'TIADA MAKLUMAT' THEN NVL(UPPER(TRIM(I2.NAMA_DAERAH)),'')
        ELSE ''
    END AS NAMA_DAERAH_PEJABAT,
    F2.ID_DAERAHURUS,
    TO_CHAR(sysdate, 'DD') AS NUM_HARI_TODAY, 
    CASE 
        WHEN TO_CHAR(sysdate, 'MM')='01' THEN 'Januari' 
        WHEN TO_CHAR(sysdate, 'MM')='02' THEN 'Febuari'
        WHEN TO_CHAR(sysdate, 'MM')='03' THEN 'Mac'
        WHEN TO_CHAR(sysdate, 'MM')='04' THEN 'April'
        WHEN TO_CHAR(sysdate, 'MM')='05' THEN 'Mei'
        WHEN TO_CHAR(sysdate, 'MM')='06' THEN 'Jun'
        WHEN TO_CHAR(sysdate, 'MM')='07' THEN 'Julai'
        WHEN TO_CHAR(sysdate, 'MM')='08' THEN 'Ogos' 
        WHEN TO_CHAR(sysdate, 'MM')='09' THEN 'September' 
        WHEN TO_CHAR(sysdate, 'MM')='10' THEN 'Oktober'
        WHEN TO_CHAR(sysdate, 'MM')='11' THEN 'November'
        WHEN TO_CHAR(sysdate, 'MM')='12' THEN 'Disember' 
    END || ' ' || TO_CHAR(sysdate, 'YYYY') AS BULAN_TODAY, 
    CASE 
        WHEN TO_CHAR(sysdate, 'FMDAY')='SUNDAY' or UPPER(TO_CHAR(sysdate, 'FMDAY'))='AHAD' THEN 'Ahad'
        WHEN TO_CHAR(sysdate, 'FMDAY')='MONDAY' or UPPER(TO_CHAR(sysdate, 'FMDAY'))='ISNIN' THEN 'Isnin'
        WHEN TO_CHAR(sysdate, 'FMDAY')='TUESDAY' or UPPER(TO_CHAR(sysdate, 'FMDAY'))='SELASA' THEN 'Selasa'
        WHEN TO_CHAR(sysdate, 'FMDAY')='WEDNESDAY' or UPPER(TO_CHAR(sysdate, 'FMDAY'))='RABU' THEN 'Rabu'
        WHEN TO_CHAR(sysdate, 'FMDAY')='THURSDAY' or UPPER(TO_CHAR(sysdate, 'FMDAY'))='KHAMIS' THEN 'Khamis'
        WHEN TO_CHAR(sysdate, 'FMDAY')='FRIDAY' or UPPER(TO_CHAR(sysdate, 'FMDAY'))='JUMAAT' THEN 'Jumaat'
        WHEN TO_CHAR(sysdate, 'FMDAY')='SATURDAY' or UPPER(TO_CHAR(sysdate, 'FMDAY'))='SABTU' THEN 'Sabtu' 
    END AS NAMA_HARI_TODAY,           
        TO_CHAR(sysdate, 'dd/mm/yyyy') AS TARIKH_TODAY_FULL 
        
    FROM
        TBLPPKOB A,
        TBLPFDFAIL B, 
        TBLPPKPERMOHONANSIMATI C, 
        TBLPPKPERMOHONAN D, 
        TBLRUJNEGERI E,
        TBLPPKKEPUTUSANPERMOHONAN G, 
        TBLPPKSIMATI H,
        TBLRUJDAERAH J,
        TBLRUJBANDAR K,
        TBLRUJPEJABATURUSAN F2,
        TBLRUJPEJABATJKPTG G2,
        TBLRUJBANDAR H2, 
        TBLRUJDAERAH I2,
        TBLRUJNEGERI J2,
        dual,
        (SELECT      
            CASE 
             WHEN TBLPPKSIMATI.NO_KP_BARU IS NULL AND TBLPPKSIMATI.NO_KP_LAMA IS NOT NULL THEN  TBLPPKSIMATI.NO_KP_LAMA
             WHEN TBLPPKSIMATI.NO_KP_BARU IS NULL AND TBLPPKSIMATI.NO_KP_LAMA IS NULL THEN  TBLPPKSIMATI.NO_KP_LAIN
             WHEN TBLPPKSIMATI.NO_KP_BARU IS NULL AND TBLPPKSIMATI.NO_KP_LAIN IS NULL THEN  TBLPPKSIMATI.NO_KP_LAMA 
             ELSE TBLPPKSIMATI.NO_KP_BARU
            END || '' ||     
            CASE 
             WHEN TBLPPKSIMATI.NO_KP_BARU IS NOT NULL AND TBLPPKSIMATI.NO_KP_LAMA IS NOT NULL THEN  TBLPPKSIMATI.NO_KP_LAMA
            END || '' ||     
            CASE 
             WHEN TBLPPKSIMATI.NO_KP_BARU IS  NULL AND TBLPPKSIMATI.NO_KP_LAMA IS NOT NULL THEN  TBLPPKSIMATI.NO_KP_LAIN
            END AS NO_KP1 , ID_SIMATI      
        FROM TBLPPKSIMATI ) AAA
    
WHERE   D.ID_PERMOHONAN         = C.ID_PERMOHONAN
    AND D.ID_FAIL               = B.ID_FAIL
    AND D.ID_PERMOHONAN         = G.ID_PERMOHONAN
    AND D.ID_PERMOHONAN         = C.ID_PERMOHONAN
    AND C.ID_SIMATI             = H.ID_SIMATI
    AND H.ID_SIMATI             = A.ID_SIMATI
    AND H.ID_SIMATI             = C.ID_SIMATI
    AND C.ID_SIMATI             = AAA.ID_SIMATI
    AND J.ID_DAERAH(+)          = D.ID_DAERAHMHN
    AND K.ID_BANDAR(+)          = A.ID_BANDAR
    AND F2.ID_PEJABATJKPTG      = G2.ID_PEJABATJKPTG
    AND F2.ID_JENISPEJABAT      = G2.ID_JENISPEJABAT
    AND G2.ID_SEKSYEN           = 2
    AND F2.ID_JENISPEJABAT      = 22
    AND G2.ID_BANDAR            = H2.ID_BANDAR(+)
    AND G2.ID_DAERAH            = I2.ID_DAERAH(+)
    AND G2.ID_NEGERI            = J2.ID_NEGERI(+)
    AND G2.ID_NEGERI(+)         = A.ID_NEGERI
    AND F2.ID_DAERAHURUS(+)     = K.ID_DAERAH
    AND F2.ID_PEJABATJKPTG     <> $P!{idpejabatjkptg}
    AND B.ID_FAIL               = $P!{idfail}]]></queryString>

