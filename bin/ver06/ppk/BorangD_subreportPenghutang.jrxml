       ,'' nama_negeri      
      --ROW_NUMBER() OVER (ORDER BY d.id_penghutang ASC )AS BIL
      
from  tblppkpenghutang d,
      tblpfdfail a,
      tblppkpermohonan b,
      tblppkpermohonansimati c,
      TBLPPKKEPUTUSANPERMOHONAN e,
      tblppkperbicaraan f,
      tblppkpemohon g,
      TBLPPKSIMATI h,
      tblrujnegeri,
      tblrujbandar      
