AND J.ID_NEGERI = I2.ID_NEGERI
AND F.ID_MAHKAMAH = J.ID_PEJABAT
AND B.ID_HAKMILIK = E.ID_HAKMILIK
AND G.ID_NEGERI = TBLRUJNEGERI.ID_NEGERI
