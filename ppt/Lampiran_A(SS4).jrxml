	<queryString><![CDATA[SELECT DISTINCT B.FLAG_SEGERA,INITCAP(C.NAMA_PEJABAT) AS NAMA_PEJABAT,replace(NVL(C.ALAMAT1,''),'&','&#38;') AS ALAMAT1,replace(NVL(C.ALAMAT2,''),'&','&#38;') AS ALAMAT2,replace(NVL(C.ALAMAT3,''),'&','&#38;') AS ALAMAT3,NVL(C.POSKOD,'') AS POSKOD, INITCAP(I.NAMA_DAERAH) AS DAERAH, K.KETERANGAN AS BANDAR,C2.NAMA_NEGERI, 
    INITCAP(D.NAMA_KEMENTERIAN) AS NAMA_KEMENTERIAN,replace(NVL(D.ALAMAT1,''),'&','&#38;') AS ALAMATK1,replace(NVL(D.ALAMAT2,''),'&','&#38;') AS ALAMATK2,replace(NVL(D.ALAMAT3,''),'&','&#38;') AS ALAMATK3,NVL(D.POSKOD,'') AS POSKODK,D2.ID_NEGERI,D2.NAMA_NEGERI, B.TUJUAN,TO_CHAR(B.TARIKH_KEHENDAKI,'dd/MM/yyyy') AS TARIKH_KEHENDAKI,INITCAP(E.NAMA_DAERAH) AS NAMA_DAERAH,INITCAP(F.NAMA_AGENSI) AS NAMA_AGENSI,D2.NAMA_NEGERI AS NEGERI,
    TO_CHAR(SYSDATE,'dd') AS HARI,
    CASE
        WHEN TO_CHAR(SYSDATE,'MM') = '01' THEN 'Januari'
        WHEN TO_CHAR(SYSDATE,'MM') = '02' THEN 'Februari'
        WHEN TO_CHAR(SYSDATE,'MM') = '03' THEN 'Mac'
        WHEN TO_CHAR(SYSDATE,'MM') = '04' THEN 'April'
        WHEN TO_CHAR(SYSDATE,'MM') = '05' THEN 'Mei'
        WHEN TO_CHAR(SYSDATE,'MM') = '06' THEN 'Jun'
        WHEN TO_CHAR(SYSDATE,'MM') = '07' THEN 'Julai'
        WHEN TO_CHAR(SYSDATE,'MM') = '08' THEN 'Ogos'
        WHEN TO_CHAR(SYSDATE,'MM') = '09' THEN 'September'
        WHEN TO_CHAR(SYSDATE,'MM') = '10' THEN 'Oktober'
        WHEN TO_CHAR(SYSDATE,'MM') = '11' THEN 'November'
        WHEN TO_CHAR(SYSDATE,'MM') = '12' THEN 'Disember'
    END AS BULAN, J.NAMA_DAERAH AS DAERAH_PERMOHONAN,
    TO_CHAR(SYSDATE,'yyyy') AS TAHUN , INITCAP(G.NAMA_NEGERI) AS NNN, B.FLAG_PERUNTUKAN, TBLRUJNEGERI.NAMA_NEGERI AS NAMA_NEGERIQ
    FROM TBLPFDFAIL A, TBLPPTPERMOHONAN B, TBLRUJPEJABAT C, TBLRUJKEMENTERIAN D, TBLRUJDAERAH E, TBLRUJAGENSI F, TBLRUJNEGERI C2, TBLRUJNEGERI D2 , TBLRUJNEGERI G, TBLPPTHAKMILIK H, TBLRUJDAERAH I, TBLRUJNEGERI,
    TBLRUJDAERAH J, TBLRUJBANDAR K
    WHERE B.ID_FAIL= A.ID_FAIL 
    AND B.ID_DAERAH = J.ID_DAERAH
    AND TBLRUJNEGERI.ID_NEGERI = A.ID_NEGERI      
    AND B.ID_PERMOHONAN = H.ID_PERMOHONAN
    AND A.ID_KEMENTERIAN = D.ID_KEMENTERIAN 
    AND B.ID_DAERAH = E.ID_DAERAH 
    AND B.ID_AGENSI = F.ID_AGENSI(+) 
    AND D.ID_NEGERI = D2.ID_NEGERI 
    AND C.ID_JENISPEJABAT = 1     
    AND C.ID_NEGERI = CASE 
                         WHEN A.ID_NEGERI = '15' OR A.ID_NEGERI = '16' THEN 14
                         ELSE A.ID_NEGERI
                      END 
    AND G.ID_NEGERI = CASE 
                         WHEN A.ID_NEGERI = '15' OR A.ID_NEGERI = '16' THEN 14
                         ELSE A.ID_NEGERI
                      END 
    AND C.ID_NEGERI = C2.ID_NEGERI
    AND C.ID_DAERAH = I.ID_DAERAH(+)
    AND C.ID_BANDAR = K.ID_BANDAR(+)
    AND A.ID_FAIL = $P!{idfail}]]></queryString>

