	<field name="TUJUAN" class="java.lang.String"/>
	<field name="PERKARA_RUJUKAN" class="java.lang.String"/>
	<field name="KEPUTUSAN_MAHKAMAH" class="java.math.BigDecimal"/>
	<field name="PERINTAH_MAHKAMAH" class="java.lang.String"/>
