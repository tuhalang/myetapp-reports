	<queryString><![CDATA[SELECT DISTINCT A.NO_FAIL,B.NO_RUJUKAN_PTG,B.TUJUAN,

SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN)) AS LUAS_SELURUH,
        
TO_CHAR(SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_SO),'999,999,999.99') AS AWARD_PTD,
       
     
CASE WHEN 
    
SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_JPPH) >
        
SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_SO)
        
THEN 

TO_CHAR(SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_JPPH) -
        
SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_SO),'999,999,999.99')

ELSE '0'

END AS TAMBAHAN,


CASE
WHEN

SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_JPPH) >
        
SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_SO)
        
THEN
      
TO_CHAR((SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_SO) /
        
SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_JPPH) * 100),'99.9')

END AS KENAIKAN,

TO_CHAR(SUM((SELECT SUM( 
  CASE WHEN TBLPPTHAKMILIK.ID_UNITLUASAMBIL_CONVERT = '1' THEN TBLPPTHAKMILIK.LUAS_AMBIL * 1
                    ELSE TBLPPTHAKMILIK.LUAS_AMBIL / 10000
                    END
                    )
        FROM TBLPFDFAIL X, TBLPPTHAKMILIK, TBLPPTPERMOHONAN
        WHERE TBLPPTHAKMILIK.ID_HAKMILIK = C.ID_HAKMILIK
        AND X.ID_FAIL = TBLPPTPERMOHONAN.ID_FAIL
        AND TBLPPTPERMOHONAN.ID_PERMOHONAN = TBLPPTHAKMILIK.ID_PERMOHONAN) * D.HARGA_SEUNIT_JPPH),'999,999,999.99') AS HARGA_JPPH,
        
TO_CHAR(F.TARIKH_SEDIA_AWARD,'dd/mm/yyyy') AS TARIKH_SEDIA_AWARD, G.KETERANGAN_SUBAWARD, TO_CHAR(G.JUMLAH_SUBAWARD,'999,999,999.99') AS LAIN_LAIN_KOS






FROM 
TBLPFDFAIL A,
TBLPPTPERMOHONAN B,
TBLPPTHAKMILIK C,
TBLPPTTANAH D,
TBLPPTSIASATAN E,
TBLPPTAWARD F,
TBLPPTSUBAWARD G



WHERE
A.ID_FAIL = B.ID_FAIL
AND B.ID_PERMOHONAN = C.ID_PERMOHONAN
AND C.ID_HAKMILIK = D.ID_HAKMILIK
AND B.ID_PERMOHONAN = E.ID_PERMOHONAN
AND (SELECT MAX(Y.ID_SIASATAN) FROM TBLPPTSIASATAN Y WHERE Y.ID_PERMOHONAN = B.ID_PERMOHONAN) = F.ID_SIASATAN
AND C.ID_HAKMILIK = E.ID_HAKMILIK
AND G.ID_AWARD = F.ID_AWARD
AND A.ID_NEGERI = $P!{id_negeri}



GROUP BY A.NO_FAIL,B.NO_RUJUKAN_PTG,B.TUJUAN,F.TARIKH_SEDIA_AWARD,G.KETERANGAN_SUBAWARD,G.JUMLAH_SUBAWARD]]></queryString>

