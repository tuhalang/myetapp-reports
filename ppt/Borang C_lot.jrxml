from tblpfdfail f , TBLPPTPERMOHONAN B, TBLPPTHAKMILIK C, TBLRUJMUKIM M
where f.id_fail = $P!{id_Fail}
AND F.ID_FAIL = B.ID_FAIL
AND B.ID_PERMOHONAN = C.ID_PERMOHONAN
AND C.ID_MUKIM = M.ID_MUKIM]]></queryString>

