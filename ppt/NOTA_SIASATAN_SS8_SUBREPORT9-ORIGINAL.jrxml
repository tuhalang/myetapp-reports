    FROM TBLPPTAWARD A, TBLPPTSUBAWARD B
    WHERE A.ID_AWARD = B.ID_AWARD
    AND B.FLAG_JENIS_AWARD = 'BAYAR_LAIN'
    AND A.ID_AWARD = $P!{idSubaward}]]></queryString>

