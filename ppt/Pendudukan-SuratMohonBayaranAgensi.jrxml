	<queryString>
		<![CDATA[SELECT DISTINCT A.ID_FAIL,A.NO_FAIL,B.NO_RUJUKAN_PTG,H.ID_HAKMILIK,H.NO_SUBJAKET, B.ID_DAERAH,
    --TO_CHAR(G.TARIKH_BORANGG,'DD.MM.YYYY') AS TARIKH_BORANGG,
    D2.ID_NEGERI,
TO_CHAR(SYSDATE,'DD','NLS_CALENDAR=''ENGLISH HIJRAH''') AS HARI_ISLAM,
    CASE
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 01 THEN 'Muharam'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 02 THEN 'Safar'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 03 THEN 'Rabiulawal'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 04 THEN 'Rabiulakhir'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 05 THEN 'Jamadilawal'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 06 THEN 'Jamadilakhir'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 07 THEN 'Rejab'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 08 THEN 'Syaaban'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 09 THEN 'Ramadhan'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 10 THEN 'Syawal'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 11 THEN 'Zulkaedah'
        WHEN TO_CHAR(SYSDATE,'MM','NLS_CALENDAR=''ENGLISH HIJRAH''') = 12 THEN 'Zulhijjah'
    END AS BULAN_ISLAM, TO_CHAR(SYSDATE,'RRRR','NLS_CALENDAR=''ENGLISH HIJRAH''') AS TAHUN_ISLAM,
    TO_CHAR(SYSDATE, 'DD') AS HARI_SURAT,
    CASE
       WHEN TO_CHAR (SYSDATE, 'MM') = '01' THEN 'Januari'
       WHEN TO_CHAR (SYSDATE, 'MM') = '02' THEN 'Februari'
       WHEN TO_CHAR (SYSDATE, 'MM') = '03' THEN 'Mac'
       WHEN TO_CHAR (SYSDATE, 'MM') = '04' THEN 'April'
       WHEN TO_CHAR (SYSDATE, 'MM') = '05' THEN 'Mei'
       WHEN TO_CHAR (SYSDATE, 'MM') = '06' THEN 'Jun'
       WHEN TO_CHAR (SYSDATE, 'MM') = '07' THEN 'Julai'
       WHEN TO_CHAR (SYSDATE, 'MM') = '08' THEN 'Ogos'
       WHEN TO_CHAR (SYSDATE, 'MM') = '09' THEN 'September'
       WHEN TO_CHAR (SYSDATE, 'MM') = '10' THEN 'Oktober'
       WHEN TO_CHAR (SYSDATE, 'MM') = '11' THEN 'November'
       WHEN TO_CHAR (SYSDATE, 'MM') = '12' THEN 'Disember'
       ELSE 'TIADA'
    END AS BULAN_SURAT,
    TO_CHAR(SYSDATE, 'YYYY') AS TAHUN_SURAT,
    UPPER(B.TUJUAN) AS TUJUAN,E.NAMA_MUKIM,F.NAMA_DAERAH,--W.NO_WARTA,
 	INITCAP(D2.NAMA_NEGERI_PENUH) AS NEGERIBWH,
    --TO_CHAR(W.TARIKH_WARTA,'DD.MM.YYYY') AS TARIKH_WARTA,
CASE
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NULL AND I.ALAMAT1 IS NULL THEN ''
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NULL AND I.ALAMAT2 IS NULL AND I.ALAMAT3 IS NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;') || chr(10) ||
REPLACE(REPLACE((I.POSKOD),','),'&','&#38;') || ' ' ||
REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NULL AND I.ALAMAT2 IS NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;') ||', ' || chr(10) ||
REPLACE(REPLACE((trim(I.ALAMAT3)),','),'&','&#38;') || chr(10) ||' '||
REPLACE(REPLACE((trim(I.POSKOD)),','),'&','&#38;') || ' ' ||
REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NULL AND I.ALAMAT3 IS NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;') ||', ' || REPLACE(REPLACE((trim(I.ALAMAT2)),','),'&','&#38;') || chr(10) ||' '|| REPLACE(REPLACE((trim(I.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NULL AND I.ALAMAT3 IS NOT NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;')||', ' || chr(10) ||
REPLACE(REPLACE((trim(I.ALAMAT2)),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT3)),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NOT NULL AND I.ALAMAT1 IS NULL THEN ''
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NOT NULL AND I.ALAMAT2 IS NULL AND I.ALAMAT3 IS NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(INITCAP(C.NAMA_AGENSI))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((I.POSKOD),','),'&','&#38;') || ' ' || REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NOT NULL AND I.ALAMAT2 IS NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(INITCAP(C.NAMA_AGENSI))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;') ||chr(10) || REPLACE(REPLACE((trim(I.ALAMAT3)),','),'&','&#38;') || chr(10) ||REPLACE(REPLACE((trim(I.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NOT NULL AND I.ALAMAT3 IS NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(INITCAP(C.NAMA_AGENSI))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;') ||chr(10) || REPLACE(REPLACE((trim(I.ALAMAT2)),','),'&','&#38;') || chr(10) ||REPLACE(REPLACE((trim(I.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   WHEN I.NAMA_KEMENTERIAN IS NOT NULL AND C.NAMA_AGENSI IS NOT NULL AND I.ALAMAT3 IS NOT NULL THEN REPLACE(REPLACE((trim(INITCAP(I.NAMA_KEMENTERIAN))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(INITCAP(C.NAMA_AGENSI))),','),'&','&#38;') || chr(10) || REPLACE(REPLACE((trim(I.ALAMAT1)),','),'&','&#38;')||chr(10) || REPLACE(REPLACE((trim(I.ALAMAT2)),','),'&','&#38;') || ', '||chr(10)|| REPLACE(REPLACE((trim(I.ALAMAT3)),','),'&','&#38;') || chr(10)|| REPLACE(REPLACE((trim(I.POSKOD)),','),'&','&#38;') || ' ' || REPLACE(REPLACE(INITCAP((D.NAMA_NEGERI)),','),'&','&#38;')
   ELSE ''
END AS ALAMAT_KEMENTERIAN,
INITCAP(D.NAMA_NEGERI)AS NAMA_NEGERI,
UPPER(D2.NAMA_NEGERI)AS NAMA_NEGERI_UNIT,
INITCAP(D2.NAMA_NEGERI) AS NAMA_NEGERI_BAWAH,
CASE
        WHEN '1'/*$P!{id_jawatan}*/ = 1 THEN 'Pengarah'
        WHEN '4'/*$P!{id_jawatan}*/ = 4 THEN 'Pengarah'
        ELSE 'b.p. Pengarah'
    END AS PENGARAH, F.NAMA_DAERAH AS DAERAH
FROM TBLPFDFAIL A,
TBLPPTPERMOHONAN B,
TBLPPTHAKMILIK H,
--TBLPPTWARTA W,
--TBLPPTBORANGG G,
TBLPPTSIASATAN S,
TBLRUJMUKIM E,
TBLRUJDAERAH F,
TBLRUJKEMENTERIAN I,
TBLRUJAGENSI C,
TBLRUJNEGERI D,
TBLRUJNEGERI D2
WHERE A.ID_FAIL = B.ID_FAIL
AND B.ID_PERMOHONAN = H.ID_PERMOHONAN
--AND B.ID_PERMOHONAN = W.ID_PERMOHONAN
AND H.ID_PERMOHONAN = S.ID_PERMOHONAN
AND S.ID_SIASATAN = (SELECT MAX(S1.ID_SIASATAN) FROM TBLPPTSIASATAN S1 WHERE H.ID_HAKMILIK = S1.ID_HAKMILIK)
--AND S.ID_SIASATAN = G.ID_SIASATAN(+)
AND H.ID_MUKIM = E.ID_MUKIM(+)
AND B.ID_DAERAH = F.ID_DAERAH
--AND W.ID_WARTA = (SELECT MAX(W1.ID_WARTA) FROM TBLPPTWARTA W1 WHERE B.ID_PERMOHONAN = W1.ID_PERMOHONAN)
AND D2.ID_NEGERI = CASE
                         WHEN A.ID_NEGERI = '15' OR A.ID_NEGERI = '16' THEN 14
                         ELSE A.ID_NEGERI
                      END
AND C.ID_AGENSI(+) = B.ID_AGENSI
AND D.ID_NEGERI(+) = I.ID_NEGERI
AND I.ID_KEMENTERIAN(+) = A.ID_KEMENTERIAN
AND H.ID_HAKMILIK = $P!{id_hakmilik}]]>
	</queryString>
