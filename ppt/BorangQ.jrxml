END AS BULAN_MULA,
TO_CHAR(H.TARIKH_MULA,'YYYY') AS TAHUN_MULA,
TO_CHAR(H.TARIKH_AKHIR,'DD') AS HARI_AKHIR,
CASE
    WHEN TO_CHAR(H.TARIKH_AKHIR,'MM') = '01' THEN 'Januari'
