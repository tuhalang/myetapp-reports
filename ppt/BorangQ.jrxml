    WHEN TO_CHAR(A.TARIKH_PENDUDUKAN_MULA,'MM') = '10' THEN 'Oktober'
    WHEN TO_CHAR(A.TARIKH_PENDUDUKAN_MULA,'MM') = '11' THEN 'November'
    WHEN TO_CHAR(A.TARIKH_PENDUDUKAN_MULA,'MM') = '12' THEN 'Disember'
