    WHEN TO_CHAR(SS.TARIKH_SIASATAN,'MM') = '03' THEN 'Mac'
    WHEN TO_CHAR(SS.TARIKH_SIASATAN,'MM') = '04' THEN 'April'
    WHEN TO_CHAR(SS.TARIKH_SIASATAN,'MM') = '05' THEN 'Mei'
