END AS BULAN_MULA,
TO_CHAR(A.TARIKH_PENDUDUKAN_MULA,'YYYY') AS TAHUN_MULA,
TO_CHAR(A.TARIKH_PENDUDUKAN_AKHIR,'DD') AS HARI_AKHIR,
CASE
    WHEN TO_CHAR(A.TARIKH_PENDUDUKAN_AKHIR,'MM') = '01' THEN 'Januari'
