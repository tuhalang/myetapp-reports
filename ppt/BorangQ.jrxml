END AS BULAN,
TO_CHAR(SYSDATE,'YYYY') AS TAHUN,
TO_CHAR(SS.TARIKH_SIASATAN,'DD') AS HARI_SIASATAN,
CASE
    WHEN TO_CHAR(SS.TARIKH_SIASATAN,'MM') = '01' THEN 'Januari'
    WHEN TO_CHAR(SS.TARIKH_SIASATAN,'MM') = '02' THEN 'Februari'
