AND SS.ID_BANDAR = SSB.ID_BANDAR(+)
AND B.ID_LOT = JL.ID_LOT(+)
AND D.ID_PIHAKBERKEPENTINGAN = C.ID_PIHAKBERKEPENTINGAN
AND E.ID_NEGERI(+) = C.ID_NEGERI
