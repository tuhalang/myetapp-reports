	<queryString><![CDATA[SELECT A.NO_FAIL, B.TUJUAN, C.NAMA_NEGERI,B.ID_PERMOHONAN,
(SELECT COUNT(*) FROM TBLPPTHAKMILIK E WHERE E.ID_PERMOHONAN = B.ID_PERMOHONAN) AS TOTAL_LOT
FROM TBLPFDFAIL A, TBLPPTPERMOHONAN B, TBLRUJNEGERI C, TBLPPTHAKMILIK D,TBLPPTPERMINTAANUKUR E
WHERE A.ID_FAIL = B.ID_FAIL
AND E.ID_HAKMILIK = D.ID_HAKMILIK
AND B.ID_PERMOHONAN = D.ID_PERMOHONAN
AND A.ID_SUBURUSAN = '52'
AND A.ID_NEGERI = C.ID_NEGERI
AND (D.LUAS_LOT - D.LUAS_AMBIL) <> 0
AND A.ID_NEGERI = $P!{id_negeri} --PARAMETER ID_NEGERI
AND TO_CHAR(B.TARIKH_PERMOHONAN,'YYYY') = $P!{tahun} --PARAMETER TAHUN]]></queryString>

