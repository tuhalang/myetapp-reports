	<queryString><![CDATA[SELECT ROWNUM AS TURUTAN,ID_MUKIM,
--NAMA_MUKIM,
(regexp_replace(INITCAP(REGEXP_REPLACE(UPPER(NAMA_MUKIM), 
'(^| )(M?M?M?(CM|CD|D?C?C?C?)(XC|XL|L?X?X?X?)(IX|IV|V?I?I?I?))($| )', ' (\2)')),
'\([[:alnum:]]+\)',upper(regexp_substr(INITCAP(REGEXP_REPLACE(UPPER(NAMA_MUKIM),
'(^| )(M?M?M?(CM|CD|D?C?C?C?)(XC|XL|L?X?X?X?)(IX|IV|V?I?I?I?))($| )', ' (\2)')),'\((.*?)\)',1,1,null,1))))
as NAMA_MUKIM,
ID_HAKMILIK,NO_LOT,NO_PT,NO_HAKMILIK,KOD_JENIS_HAKMILIK,LUAS_AMBIL,UNIT_AMBIL,LUAS_AMBIL_H,LUAS_LOT_H,LUAS_LOT,UNIT_LOT,JUMLAH,KATEGORI_TANAH
FROM (
SELECT to_char(M.ID_MUKIM) as id_mukim, INITCAP(REPLACE(M.NAMA_MUKIM,'-','- ')) AS NAMA_MUKIM, TO_CHAR(H.ID_HAKMILIK) AS ID_HAKMILIK,
CASE  
    WHEN H.NO_LOT IS NOT NULL AND H.NO_PT IS NULL THEN H.NO_LOT  
    WHEN H.NO_LOT IS NULL AND H.NO_PT IS NOT NULL THEN  JL.KETERANGAN  || CHR(32) || H.NO_PT   
    WHEN H.NO_LOT IS NOT NULL AND H.NO_PT IS NOT NULL THEN JL.KETERANGAN  || H.NO_PT || CHR(32) || CHR(40) || H.NO_LOT  || CHR(41) 
    ELSE  ''  
END AS NO_LOT,H.NO_PT,H.NO_HAKMILIK,
HM.KOD_JENIS_HAKMILIK, 
TO_CHAR(ROUND(H.LUAS_AMBIL,4),'99990.9999') AS LUAS_AMBIL,
    case
    when h.ID_UNITLUASAMBIL_CONVERT = 1 then 'hektar'
    when h.ID_UNITLUASAMBIL_CONVERT = 2 then 'meter persegi'
    end as unit_AMBIL,
REPLACE(TO_CHAR(case
    when h.ID_UNITLUASAMBIL_CONVERT = 1 then TO_CHAR(ROUND(H.LUAS_AMBIL,4),'99990.9999')
    when h.ID_UNITLUASAMBIL_CONVERT = 2 then TO_CHAR(ROUND((H.LUAS_AMBIL/10000),4),'99990.9999')
    end),' ') as LUAS_AMBIL_H,
REPLACE(TO_CHAR(case
    when h.ID_UNITLUASLOT_CONVERT = 1 then TO_CHAR(ROUND(H.LUAS_LOT,4),'99990.9999')
    when h.ID_UNITLUASLOT_CONVERT = 2 then TO_CHAR(ROUND((H.LUAS_LOT/10000),4),'99990.9999')
    end),' ') as LUAS_LOT_H,
TO_CHAR(ROUND(H.LUAS_LOT,4),'99990.9999') AS LUAS_LOT,
    case
    when h.ID_UNITLUASLOT_CONVERT = 1 then 'hektar'
    when h.ID_UNITLUASLOT_CONVERT = 2 then 'meter persegi'
    end as unit_LOT,'' AS JUMLAH, INITCAP(KT.KETERANGAN) AS KATEGORI_TANAH
FROM TBLPPTPERMOHONAN P,TBLPFDFAIL F,
TBLPPTHAKMILIK H,TBLRUJMUKIM M , TBLRUJJENISHAKMILIK HM, TBLRUJLOT JL, TBLRUJKATEGORI KT
WHERE P.ID_FAIL= F.ID_FAIL
AND P.ID_PERMOHONAN = H.ID_PERMOHONAN
AND H.ID_MUKIM = M.ID_MUKIM
AND KT.ID_KATEGORI(+) = H.ID_KATEGORITANAH
AND H.ID_JENISHAKMILIK = HM.ID_JENISHAKMILIK(+)
AND H.ID_LOT = JL.ID_LOT(+)
AND F.ID_FAIL = $P!{id_Fail}
AND M.ID_MUKIM = $P!{id_mukim}
ORDER BY LPAD(H.no_lot,20) asc, LPAD(H.no_pt,20) asc, LPAD(NO_LOT,20) asc, LPAD(H.no_subjaket,20) asc
)]]></queryString>

