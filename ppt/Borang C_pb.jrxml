<!DOCTYPE jasperReport PUBLIC "//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport
		 name="Borang C_pb"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Portrait"
		 pageWidth="535"
		 pageHeight="842"
		 columnWidth="535"
		 columnSpacing="0"
		 leftMargin="0"
		 rightMargin="0"
		 topMargin="0"
		 bottomMargin="0"
		 whenNoDataType="NoPages"
		 isTitleNewPage="false"
		 isSummaryNewPage="false">
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="id_bahagian" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT INITCAP(REPLACE(REPLACE(D.NAMA_PB,'&','&#38;'),'@','&#64;')) AS NAMA_PB, PFD.ID_NEGERI,
TO_CHAR(CASE WHEN JPB.ID_JENISNOPB = 1 AND D.NO_PB IS NOT NULL
THEN SUBSTR(D.NO_PB,1,6) || '-' || SUBSTR(D.NO_PB,7,2) || '-' || SUBSTR(D.NO_PB,9,4)
WHEN JPB.ID_JENISNOPB = 1 AND D.NO_PB IS NULL THEN ''
ELSE D.NO_PB END) AS NO_PB,
INITCAP(JPB.KETERANGAN) AS JENIS_NO_PB,
 TO_CHAR(C.SYER_ATAS) AS SYER_ATAS, TO_CHAR(C.SYER_BAWAH) AS SYER_BAWAH
    FROM TBLPFDFAIL PFD, TBLPPTPERMOHONAN PP, TBLPPTHAKMILIK A, TBLPPTHAKMILIKPB B,TBLRUJJENISNOPB JPB, TBLPPTBAHAGIANPB C, TBLPPTPIHAKBERKEPENTINGAN D, TBLRUJJENISPB E
    WHERE A.ID_HAKMILIK = B.ID_HAKMILIK
    AND PP.ID_FAIL = PFD.ID_FAIL
    AND A.ID_PERMOHONAN = PP.ID_PERMOHONAN
    AND B.ID_BAHAGIANPB = C.ID_BAHAGIANPB(+)
    AND D.ID_JENISNOPB =  JPB.ID_JENISNOPB(+)
    AND B.ID_PIHAKBERKEPENTINGAN = D.ID_PIHAKBERKEPENTINGAN
    AND B.ID_JENISPB = E.ID_JENISPB(+)
   -- AND E.JENIS_DAFTAR_PB = 'Y'
    --AND B.FLAG_BORANGC = '1'
    AND B.ID_BAHAGIANPB = $P!{id_bahagian}
	ORDER BY E.ID_JENISPB ASC]]></queryString>

	<field name="NAMA_PB" class="java.lang.String"/>
	<field name="ID_NEGERI" class="java.math.BigDecimal"/>
	<field name="NO_PB" class="java.lang.String"/>
	<field name="JENIS_NO_PB" class="java.lang.String"/>
	<field name="SYER_ATAS" class="java.lang.String"/>
	<field name="SYER_BAWAH" class="java.lang.String"/>

		<background>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</background>
		<title>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</title>
		<pageHeader>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</pageHeader>
		<columnHeader>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</columnHeader>
		<detail>
			<band height="9"  isSplitAllowed="true" >
				<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true" evaluationTime="Now" hyperlinkType="None"  hyperlinkTarget="Self" >
					<reportElement
						mode="Transparent"
						x="0"
						y="0"
						width="176"
						height="9"
						key="textField-2"
						positionType="Float"
						isRemoveLineWhenBlank="true">
							<printWhenExpression><![CDATA[new Boolean($F{ID_NEGERI}.intValue()!=5)]]></printWhenExpression>
						</reportElement>
					<box></box>
					<textElement textAlignment="Left" verticalAlignment="Top" markup="styled">
						<font fontName="Cambria" size="10"/>
					</textElement>
				<textFieldExpression   class="java.lang.String"><![CDATA["<style isBold='true' pdfFontName='Helvetica-Bold'>"+($F{NAMA_PB} != null ?""+$F{NAMA_PB}+"":"")+"</style>"
+($F{NO_PB} != null ? "\n" +$F{NO_PB}:"")]]></textFieldExpression>
				</textField>
			</band>
		</detail>
		<columnFooter>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</columnFooter>
		<pageFooter>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</pageFooter>
		<summary>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</summary>
</jasperReport>
