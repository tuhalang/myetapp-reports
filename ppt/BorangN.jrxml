				<reportElement uuid="0cad6fcc-164d-49b0-b90e-acfd61e1676c" key="textField-1" positionType="Float" x="60" y="9" width="469" height="19"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="1_1_2"/>
				</textElement>
				<textFieldExpression><![CDATA["<style isBold='true' pdfFontName='Helvetica-Bold'>"+"BORANG N"+"</style>"
+"\nAKTA PENGAMBILAN TANAH, 1960"
+"\n<style pdfFontName='Helvetica-Oblique'>"+"[Seksyen 38]"+"</style>"
+"\n<style isBold='true' pdfFontName='Helvetica-Bold'>"+"PERMOHONAN SUPAYA BANTAHAN DIRUJUKKAN KEPADA MAHKAMAH"+"</style>"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="f0bd4f5b-15fd-4816-89f6-9d12e8868277" key="textField-2" positionType="Float" x="60" y="34" width="469" height="20"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="1_1_2"/>
				</textElement>
				<textFieldExpression><![CDATA["\n\n "+"Kepada : "
+"\n                Pentadbir Tanah Daerah "+$F{NAMA_DAERAH}
+"\n                PROSIDING PENGAMBILAN NO.  ..................."
+"\n "
+"\nSaya/Kami "+$F{NAMA_PB}+" yang beralamat di "+$F{ALAMAT_PENUH_PB}+" dengan ini membuat suatu bantahan terhadap award Pentadbir Tanah "+$F{NAMA_DAERAH}+" bertarikh "+$F{HARI_BORANGN}+" haribulan "+$F{BULAN_BORANGN}+" "+$F{TAHUN_BORANGN}+" berkenaan dengan tanah Lot "+$F{NO_LOT}+" Hakmilik "+$F{NO_HAKMILIK}
+"\n"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="a424b602-08c0-41d2-8ab1-685134dd07d1" key="textField-6" positionType="Float" x="330" y="240" width="188" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="1_1_2"/>
				</textElement>
				<textFieldExpression><![CDATA["........................................."
+"\nTandatangan Pemohon"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="false">
				<reportElement uuid="ed20461d-d8a6-4b5d-a293-0d6b1288e968" key="textField-7" positionType="Float" x="62" y="240" width="257" height="15"/>
				<textElement textAlignment="Left" verticalAlignment="Top" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="Single"/>
				</textElement>
				<textFieldExpression><![CDATA["Bertarikh pada "+ $F{HARI} +" haribulan "+$F{BULAN}+" "+$F{TAHUN}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="fbc173a7-c3aa-4999-8bd1-db0b4826f06e" key="textField-8" positionType="Float" x="60" y="54" width="469" height="20"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="1_1_2"/>
				</textElement>
				<textFieldExpression><![CDATA["2.         Kepentingan saya/kami mengenai tanah tersebut ialah seperti yang berikut:"
+"\n        \t "+$F{KEPENTINGANKEATAS}
+"\n"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="1e823310-ef10-4b4a-a235-409940713b60" key="textField-9" positionType="Float" x="60" y="74" width="469" height="63"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="1_1_2"/>
				</textElement>
				<textFieldExpression><![CDATA["*3.         Bantahan saya/kami adalah terhadap: "
+"\n        \t "+ ($F{FLAG_PENERIMA_PAMPASAN}== null ? "<style isStrikeThrough ='true'>(a) ukuran tanah itu;</style>" :"(a) ukuran tanah itu; ")
+"\n        \t "+ ($F{FLAG_BAHAGIAN_PAMPASAN}== null ? "<style isStrikeThrough ='true'>(b) amaun pampasan itu;</style>" :"(b) amaun pampasan itu; ")
+"\n        \t "+ ($F{FLAG_UKUR_LUAS}== null ? "<style isStrikeThrough ='true'>(c) orang-orang yang kepadanya pampasan kena dibayar;</style>" :"(c) orang-orang yang kepadanya pampasan kena dibayar; ")
+"\n        \t "+ ($F{FLAG_PAMPASAN}== null ? "<style isStrikeThrough ='true'>(d) pembahagian pampasan itu.</style>" :"(d) pembahagian pampasan itu. ")
+"\n"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="4afe2c96-68ba-4f12-815a-aee8fdc068ff" key="textField-10" positionType="Float" x="60" y="137" width="469" height="45"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="1_1_2"/>
				</textElement>
				<textFieldExpression><![CDATA["**4.         Alasan-alasan bantahan saya/kami ialah seperti yang berikut:"
+"\n        \t "+$F{ALASAN}
+"\n"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="e5b69700-da08-4b1c-93cb-7b5d2e83c78c" key="textField-11" positionType="Float" x="60" y="182" width="469" height="20"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="styled">
					<font fontName="Cambria" size="12" isBold="false" pdfFontName="Helvetica"/>
					<paragraph lineSpacing="1_1_2"/>
				</textElement>
				<textFieldExpression><![CDATA["5.      Mengikut subseksyen 38(1) Akta Pengambilan Tanah 1960, saya dengan ini menghendaki tuan supaya merujuk perkara itu kepada Mahkamah untuk diputuskan"]]></textFieldExpression>
			</textField>
