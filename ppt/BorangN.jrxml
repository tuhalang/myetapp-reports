				</textElement>
				<textFieldExpression><![CDATA["\nKepada : "
+"\n     Pentadbir Tanah Daerah "+$F{NAMA_DAERAH}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="" isBlankWhenNull="true">
				<reportElement uuid="a424b602-08c0-41d2-8ab1-685134dd07d1" key="textField-6" positionType="Float" x="269" y="242" width="170" height="15"/>
