	<queryString>
		<![CDATA[SELECT R,id_hakmilik,id_hakmilikpb,id_negeri,no_fail,no_rujukan_ptg,no_subjaket,id_fail,no_siasatan,no_warta,tarikh_warta,hari_siasatan,bulan_siasatan,tahun_siasatan,bayar_pampasan,
hari,bulan,tahun,hari_h,bulan_h,tahun_h,nama_pb,alamat1,alamat2,alamat3,poskod,nama_bandar,nama_negeri,np,hari_warta,bulan_warta,tahun_warta,no_pb,jenis_no_pb,jumlah_award FROM
(
SELECT rownum R,c.id_hakmilik, TO_CHAR (d.id_hakmilikpb) AS id_hakmilikpb,
                   a.id_negeri, a.no_fail, b.no_rujukan_ptg, c.no_subjaket,
                   TO_CHAR (a.id_fail) AS id_fail, f.no_siasatan, g.no_warta,
                   TO_CHAR (g.tarikh_warta, 'DD.mm.yyyy') AS tarikh_warta,
                   TO_CHAR (f.tarikh_siasatan, 'DD') AS hari_siasatan,
                   CASE
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '01'
                         THEN 'Januari'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '02'
                         THEN 'Februari'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '03'
                         THEN 'Mac'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '04'
                         THEN 'April'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '05'
                         THEN 'Mei'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '06'
                         THEN 'Jun'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '07'
                         THEN 'Julai'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '08'
                         THEN 'Ogos'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '09'
                         THEN 'September'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '10'
                         THEN 'Oktober'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '11'
                         THEN 'November'
                      WHEN TO_CHAR (f.tarikh_siasatan,
                                    'MM') = '12'
                         THEN 'Disember'
                   END AS bulan_siasatan,
                   TO_CHAR (f.tarikh_siasatan, 'YYYY') AS tahun_siasatan,
                   REPLACE (TO_CHAR (j.bayar_pampasan, '999,999,990.99'),
                            ' '
                           ) AS bayar_pampasan,
                   TO_CHAR (f.tarikh_perintah, 'DD') AS hari,
                   CASE
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '01'
                         THEN 'Januari'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '02'
                         THEN 'Februari'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '03'
                         THEN 'Mac'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '04'
                         THEN 'April'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '05'
                         THEN 'Mei'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '06'
                         THEN 'Jun'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '07'
                         THEN 'Julai'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '08'
                         THEN 'Ogos'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '09'
                         THEN 'September'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '10'
                         THEN 'Oktober'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '11'
                         THEN 'November'
                      WHEN TO_CHAR (f.tarikh_perintah, 'MM') = '12'
                         THEN 'Disember'
                   END AS bulan,
                   TO_CHAR (gg.tarikh_borangh, 'DD') AS hari_h,
                   CASE
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '01'
                         THEN 'Januari'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '02'
                         THEN 'Februari'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '03'
                         THEN 'Mac'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '04'
                         THEN 'April'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '05'
                         THEN 'Mei'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '06'
                         THEN 'Jun'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '07'
                         THEN 'Julai'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '08'
                         THEN 'Ogos'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '09'
                         THEN 'September'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '10'
                         THEN 'Oktober'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '11'
                         THEN 'November'
                      WHEN TO_CHAR (gg.tarikh_borangh, 'MM') =
                                                              '12'
                         THEN 'Disember'
                   END AS bulan_h,
                   TO_CHAR (gg.tarikh_borangh, 'YYYY') AS tahun_h,
                   TO_CHAR (f.tarikh_perintah, 'YYYY') AS tahun,
                  -- INITCAP (REPLACE(REPLACE(e.nama_pb,'&','&#38;'),'@','&#64;')) AS nama_pb,
replace(replace(replace(replace(replace(initcap(replace(NAMA_PB,'''','xxxx')),'xxxx',''''),'Xxxxa','''A'),'Xxxxu','''U'), '&','&#38;'),'@','&#64;') AS NAMA_PB,
                    REPLACE(REPLACE(TRIM(INITCAP(NVL(d.alamat1,''))),'&','&#38;'),',') AS alamat1,
                   REPLACE (INITCAP (NVL (d.alamat2, '')),
                            '&',
                            '<subst>;'
                           ) AS alamat2,
                   REPLACE (INITCAP (NVL (d.alamat3, '')),
                            '&',
                            '<subst>;'
                           ) AS alamat3,
                   NVL (d.poskod, '') AS poskod,
                   INITCAP (h.keterangan) AS nama_bandar,
                   INITCAP (i.nama_negeri) AS nama_negeri,
                   INITCAP (nn.nama_negeri) AS np,
                   TO_CHAR (g.tarikh_warta, 'DD') AS hari_warta,
                   CASE
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '01'
                         THEN 'Januari'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '02'
                         THEN 'Februari'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '03'
                         THEN 'Mac'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '04'
                         THEN 'April'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '05'
                         THEN 'Mei'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '06'
                         THEN 'Jun'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '07'
                         THEN 'Julai'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '08'
                         THEN 'Ogos'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '09'
                         THEN 'September'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '10'
                         THEN 'Oktober'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '11'
                         THEN 'November'
                      WHEN TO_CHAR (g.tarikh_warta, 'MM') =
                                                          '12'
                         THEN 'Disember'
                   END AS bulan_warta,
                   TO_CHAR (g.tarikh_warta, 'YYYY') AS tahun_warta,
              TO_CHAR (CASE
                      WHEN jpb.id_jenisnopb = 1
                         THEN    SUBSTR (e.no_pb, 1, 6)
                              || '-'
                              || SUBSTR (e.no_pb, 7, 2)
                              || '-'
                              || SUBSTR (e.no_pb, 9, 4)
                      ELSE e.no_pb
                   END
                  ) AS no_pb,INITCAP (jpb.keterangan) AS jenis_no_pb,
(SELECT replace(to_char(sum (c.jumlah_subaward),'999,999,990.99'),' ')
                   FROM tblpptsubaward c
                  WHERE j.id_award = c.id_award
                    AND c.flag_jenis_award != 'BAYAR_LAIN'
                    AND c.flag_jenis_award != 'BAYAR_PENILAI') AS jumlah_award
              FROM tblpfdfail a,
                   tblpptpermohonan b,
                   tblppthakmilik c,
                   tblppthakmilikpb d,
                   tblpptpihakberkepentingan e,
                   tblpptsiasatan f,
                   tblpptwarta g,
                   tblrujbandar h,
                   tblrujnegeri i,
                   tblpptaward j,
                   tblpptborangg gg,
                   tblrujnegeri nn,
                   tblrujjenisnopb jpb
             WHERE a.id_fail = b.id_fail
               AND b.id_permohonan = c.id_permohonan
               AND c.id_hakmilik = d.id_hakmilik
               AND e.id_jenisnopb = jpb.id_jenisnopb(+)
               AND d.id_pihakberkepentingan = e.id_pihakberkepentingan
               AND c.id_hakmilik = f.id_hakmilik
               AND f.id_siasatan =
                           (SELECT MAX (id_siasatan)
                              FROM tblpptsiasatan
                             WHERE c.id_hakmilik = tblpptsiasatan.id_hakmilik)
               AND b.id_permohonan = g.id_permohonan
               AND g.id_warta =
                          (SELECT MAX (id_warta)
                             FROM tblpptwarta
                            WHERE b.id_permohonan = tblpptwarta.id_permohonan)
               AND d.id_bandar = h.id_bandar(+)
               AND d.id_negeri = i.id_negeri(+)
               AND j.id_siasatan =
                           (SELECT MAX (id_siasatan)
                              FROM tblpptsiasatan
                             WHERE c.id_hakmilik = tblpptsiasatan.id_hakmilik)
               AND d.id_hakmilikpb = j.id_hakmilikpb
               AND f.id_siasatan = gg.id_siasatan(+)
               AND nn.id_negeri = a.id_negeri(+)
               AND d.id_hakmilikpb IN (
                      SELECT DISTINCT hpb.id_hakmilikpb
                                 FROM tblppthakmilikpb hpb,
                                      tblpptaward j1,
                                      tblpptsubaward sd
                                WHERE sd.id_award = j1.id_award
                                  AND hpb.id_hakmilikpb = j1.id_hakmilikpb
                                  AND hpb.id_hakmilik = c.id_hakmilik
                                  AND sd.flag_jenis_award NOT IN ('BAYAR_LAIN','BAYAR_PENILAI'))
          AND c.id_hakmilik = $P!{id_hakmilik}
) WHERE id_hakmilik IS NOT NULL $P!{send_conditon}
ORDER BY R]]>
	</queryString>
