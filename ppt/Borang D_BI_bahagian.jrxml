,tblpptpihakberkepentingan pb
WHERE HPB.ID_BAHAGIANPB = BPB.ID_BAHAGIANPB
AND HPB.ID_HAKMILIK = HM.ID_HAKMILIK
and hpb.id_pihakberkepentingan = pb.id_pihakberkepentingan
