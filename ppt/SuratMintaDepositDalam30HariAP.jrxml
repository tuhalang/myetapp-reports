    CASE
        WHEN $P!{id_jawatan} = 1 THEN 'Pengarah'
        WHEN $P!{id_jawatan} = 4 THEN 'Pengarah'
        ELSE 'b.p. Pengarah'
    END AS PENGARAH
    FROM TBLPFDFAIL A, TBLPPTPERMOHONAN B, TBLPPTHAKMILIK C, TBLPPTPIHAKBERKEPENTINGAN E, TBLPPTBANTAHAN F, TBLPPTBORANGO G,
