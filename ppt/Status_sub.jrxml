<!DOCTYPE jasperReport PUBLIC "//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport
		 name="Status_sub"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Landscape"
		 pageWidth="850"
		 pageHeight="842"
		 columnWidth="850"
		 columnSpacing="0"
		 leftMargin="0"
		 rightMargin="0"
		 topMargin="0"
		 bottomMargin="0"
		 whenNoDataType="NoPages"
		 isTitleNewPage="false"
		 isSummaryNewPage="false">
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="ReportDir" isForPrompting="true" class="java.lang.String"/>
	<parameter name="BULAN" isForPrompting="true" class="java.lang.String"/>
	<parameter name="TAHUN" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT
TO_CHAR(CASE 
        WHEN $P!{BULAN}='1' THEN 'JANUARI'
        WHEN $P!{BULAN}='2' THEN 'FEBRUARI' 
        WHEN $P!{BULAN}='3' THEN 'MAC'
        WHEN $P!{BULAN}='4' THEN 'APRIL'
        WHEN $P!{BULAN}='5' THEN 'MEI'
        WHEN $P!{BULAN}='6' THEN 'JUN'
        WHEN $P!{BULAN}='7' THEN 'JULAI'
        WHEN $P!{BULAN}='8' THEN 'OGOS'
        WHEN $P!{BULAN}='9' THEN 'SEPTEMBER'
        WHEN $P!{BULAN}='10' THEN 'OKTOBER'
        WHEN $P!{BULAN}='11' THEN 'NOVEMBER'
        WHEN $P!{BULAN}='12' THEN 'DISEMBER'
END) AS BULAN  FROM DUAL]]></queryString>

	<field name="BULAN" class="java.lang.String"/>

		<background>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</background>
		<title>
			<band height="16"  isSplitAllowed="true" >
				<textField isStretchWithOverflow="true" isBlankWhenNull="false" evaluationTime="Now" hyperlinkType="None"  hyperlinkTarget="Self" >
					<reportElement
						x="0"
						y="0"
						width="174"
						height="16"
						key="textField-4"
						positionType="Float"/>
					<box></box>
					<textElement textAlignment="Left">
						<font pdfFontName="Helvetica-Bold" isBold="true"/>
					</textElement>
				<textFieldExpression   class="java.lang.String"><![CDATA["BULAN " +$F{BULAN}+ " / " +$P{TAHUN}]]></textFieldExpression>
				</textField>
			</band>
		</title>
		<pageHeader>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</pageHeader>
		<columnHeader>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</columnHeader>
		<detail>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</detail>
		<columnFooter>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</columnFooter>
		<pageFooter>
			<band height="0"  isSplitAllowed="true" >
			</band>
		</pageFooter>
		<summary>
