	<parameter name="id_mukim" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT G.CATATAN AS CATATAN_PU,
    CASE 
    WHEN C.NO_LOT IS NOT NULL AND C.NO_PT IS NULL THEN C.NO_LOT 
    WHEN C.NO_LOT IS NULL AND C.NO_PT IS NULL THEN F.KETERANGAN || C.NO_PT 
    WHEN C.NO_LOT IS NULL AND C.NO_PT IS NOT NULL THEN  F.KETERANGAN  || CHR(32) || C.NO_PT  
    WHEN C.NO_LOT IS NOT NULL AND C.NO_PT IS NOT NULL THEN C.NO_LOT || CHR(32) || CHR(40) || F.KETERANGAN  || CHR(32) || C.NO_PT || CHR(41) 
    ELSE ''
    END AS NO_LOTPT, C.NO_HAKMILIK, C1.KOD_JENIS_HAKMILIK,       
    REPLACE(TO_CHAR((CASE 
                        WHEN C.ID_UNITLUASLOT_CONVERT = '1' THEN C.LUAS_LOT * 1
                        ELSE C.LUAS_LOT / 10000
                     END),'999,990.9999'),' ')AS LUAS_ASAL,
    REPLACE(TO_CHAR((CASE 
                        WHEN C.ID_UNITLUASAMBIL_CONVERT = '1' THEN C.LUAS_AMBIL * 1
                        ELSE C.LUAS_AMBIL / 10000
                     END),'999,990.9999'),' ')AS LUAS_AMBIL                 
    FROM TBLPFDFAIL A, TBLPPTPERMOHONAN B, TBLPPTHAKMILIK C, TBLRUJMUKIM D, TBLRUJDAERAH E, 
    TBLRUJLOT F, TBLRUJJENISHAKMILIK C1, TBLPPTPERMINTAANUKUR G
    WHERE A.ID_FAIL = B.ID_FAIL
    AND B.ID_PERMOHONAN = C.ID_PERMOHONAN
    AND C.ID_MUKIM = D.ID_MUKIM
    AND B.ID_DAERAH = E.ID_DAERAH
    AND C.ID_LOT = F.ID_LOT(+)
    AND C.ID_JENISHAKMILIK = C1.ID_JENISHAKMILIK(+)
    AND C.ID_HAKMILIK = G.ID_HAKMILIK
    AND B.ID_PERMOHONAN = $P!{id_permohonan}
    AND C.ID_MUKIM = $P!{id_mukim}
    ORDER BY C.NO_LOT]]></queryString>

