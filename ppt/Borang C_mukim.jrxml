	<queryString><![CDATA[SELECT DISTINCT TO_CHAR(M.ID_MUKIM) AS ID_MUKIM,INITCAP(M.NAMA_MUKIM) AS NAMA_MUKIM,
INITCAP(D.NAMA_DAERAH) AS NAMA_DAERAH 
FROM TBLPPTPERMOHONAN P,TBLPFDFAIL F,TBLPPTHAKMILIK H,TBLRUJMUKIM M,TBLRUJDAERAH D
WHERE F.ID_FAIL = P.ID_FAIL
AND P.ID_PERMOHONAN = H.ID_PERMOHONAN 
AND H.ID_MUKIM = M.ID_MUKIM
AND P.ID_DAERAH = D.ID_DAERAH
    AND NVL(H.FLAG_PENARIKAN_KESELURUHAN,'0') <> 'Y'
    AND NVL(H.FLAG_PEMBATALAN_KESELURUHAN,'0') <> 'Y'
AND P.ID_FAIL = $P!{id_Fail}]]></queryString>

