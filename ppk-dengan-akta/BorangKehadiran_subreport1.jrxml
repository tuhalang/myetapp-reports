	<parameter name="idfail" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ReportDir" isForPrompting="true" class="java.lang.String"/>
	<parameter name="idParentOB" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[-- 2014/04/23
-- kemaskini 2014/04/24

SELECT distinct TBLPFDFAIL.ID_FAIL
	,GETMYIDFORMAT(GETMYID(TBLPPKOB.NO_KP_BARU,TBLPPKOB.NO_KP_LAMA,TBLPPKOB.NO_KP_LAIN)) KP_OB
    	,UPPER(nvl(TBLPPKOB.NAMA_OB,'')) AS TBLPPKOB_NAMA_OB
	,NVL(TBLPPKOB.NO_SURAT_BERANAK,'') AS NO_SURAT_BERANAK
	,NVL( TBLPPKOB.UMUR,'')  AS TBLPPKOB_UMUR
	,TBLPPKOB.STATUS_HIDUP, TBLPPKOB.LAPIS
	,CASE 
        	WHEN TBLPPKOB.FLAG_DAFTAR = 1 THEN 'Pendaftaran'
        	WHEN TBLPPKOB.FLAG_DAFTAR = 2 THEN 'Perbicaraan'
        ELSE ''
    	END AS KEHADIRAN
	,TO_CHAR(TBLPPKOB.TARIKH_MATI,'DD/MM/YYYY')AS TARIKH_MATI
    	,--PERBICARAAN_HUBUNGANWARIS(TBLPPKPERMOHONAN.ID_PERMOHONAN,TBLPPKOB.NO_KP_LAMA||TBLPPKOB.NO_KP_BARU||TBLPPKOB.NO_KP_LAIN||TBLPPKOB.JENIS_KP||TBLPPKOB.NO_SURAT_BERANAK) KETERANGAN,
	
	NVL((select TBLPPKRUJSAUDARA.KETERANGAN from TBLPPKRUJSAUDARA 
	where TBLPPKPERMOHONAN.ID_PERMOHONAN = TBLPPKPERMOHONANSIMATI.ID_PERMOHONAN
    	AND TBLPPKOB.ID_SIMATI = TBLPPKPERMOHONANSIMATI.ID_SIMATI
	AND TBLPPKRUJSAUDARA.ID_SAUDARA(+) = TBLPPKOB.ID_SAUDARA
	and rownum<=1),'')as KETERANGAN,

        NVL((select TBLPPKHUBUNGANOB.ID_PARENTOB from TBLPPKRUJSAUDARA, TBLPPKHUBUNGANOB 
	where TBLPPKOB.ID_PERMOHONANSIMATI = TBLPPKPERMOHONANSIMATI.ID_PERMOHONANSIMATI
	AND TBLPPKPERMOHONAN.ID_FAIL = TBLPFDFAIL.ID_FAIL
	AND TBLPPKHUBUNGANOB.ID_PARENTOB = TBLPPKOB.ID_OB
	AND TBLPPKPERMOHONAN.ID_PERMOHONAN = TBLPPKPERMOHONANSIMATI.ID_PERMOHONAN
	AND TBLPPKHUBUNGANOB.ID_PARENTOB = $P{idParentOB}
	and rownum<=1),'') as idParentOB
FROM
     	TBLPFDFAIL,
     	TBLPPKPERMOHONAN,
     	TBLPPKPERMOHONANSIMATI,
     	TBLPPKOB
WHERE  
TBLPPKPERMOHONAN.ID_PERMOHONAN = TBLPPKPERMOHONANSIMATI.ID_PERMOHONAN
AND TBLPPKOB.ID_PERMOHONANSIMATI = TBLPPKPERMOHONANSIMATI.ID_PERMOHONANSIMATI
AND TBLPPKPERMOHONAN.ID_FAIL = TBLPFDFAIL.ID_FAIL
AND TBLPPKOB.LAPIS <=1
AND TBLPPKOB.ID_TARAFKPTG = 1
AND TBLPFDFAIL.ID_FAIL = $P!{idfail}
--AND TBLPPKPERMOHONAN.ID_PERMOHONAN = 
--808
--937
--1183
ORDER BY
TBLPPKOB_UMUR DESC NULLS LAST]]></queryString>

