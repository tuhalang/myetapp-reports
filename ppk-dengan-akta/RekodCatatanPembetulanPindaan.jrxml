	<parameter name="tarikh_hingga" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[Select A.NO_PINDAAN, TO_DATE(A.tarikh_masuk,'DD/MM/YYYY') AS TARIKH_PINDAAN,B.NO_FAIL ,C.TUJUAN_PINDAAN, C.ID_DAERAHMHN, D.NAMA_DAERAH
from TBLPPKBORANG_HISTORY A, TBLPFDFAIL B, TBLPPKPERMOHONAN C, TBLRUJDAERAH D
 where 
 A.ID_FAIL = B.ID_FAIL
 AND A.ID_FAIL = C.ID_FAIL
 AND B.ID_FAIL = C.ID_FAIL
 and nama_borang='BorangE'
 AND A.FLAG_KEB_KEMASKINI = 1
 AND C.ID_DAERAHMHN = D.ID_DAERAH
 AND C.ID_DAERAHMHN IN ( SELECT DISTINCT U.ID_DAERAHURUS FROM TBLRUJPEJABATURUSAN U, USERS_INTERNAL UR WHERE U.ID_PEJABATJKPTG=UR.ID_PEJABATJKPTG AND UR.USER_ID=$P{userId} )
AND A.tarikh_masuk BETWEEN TO_DATE($P{tarikh_dari},'DD/MM/YYYY') AND TO_DATE($P{tarikh_hingga}, 'DD/MM/YYYY')
order by TARIKH_PINDAAN ASC]]></queryString>

